@{
    ViewBag.Title = "actualizarExpediente";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}
<head>
    <title>Actualizar Padron de Socios</title>
</head>
<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <!-- INICIO RUTA -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li class="active">Actualizar Padrón de Socios</li>
            </ul>

        </div><!-- FIN RUTA -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="cbp-mc-column">
                    <h1> Actualizar Padrón de Socios </h1>
                </div>

            </div>
            <!-- Fin titulo -->

            <!-- inicio pasos-->
            <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">
                <!-- contenido de pasos -->
                <!--------- ACORDEON ------------>

                <div id="accordion" style="border:1px solid; ">
                    <div class="card">

                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-search"></i>
                                    Criterios de Búsqueda
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                            <div class="card-body" style="padding-left:12px;padding-right:12px;">

                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-12 panel panel-default" style="width:20%; margin-left:1%; margin-top:1%;">
                                        <div class="panel-body inline-block" style="text-align:left">
                                            <label>Buscar Por:</label>
                                            &nbsp;&nbsp;&nbsp;
                                            <label><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                            &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;
                                            <label><input id="rb_sgd" type="radio" name="optradio"> Nros. SGD CUT</label>
                                        </div>
                                    </div>

                                </div>

                                <div id="conRuc" class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>RUC :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:50%" })
                                            <button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. Expediente :</label>
                                        </div>
                                        <div>
                                            <select id="cbxExpedientes" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. SGD(CUT) :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", disabled = true })
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Estado :</label>
                                        </div>
                                        <div>
                                            @{List<KeyValuePair<int, string>> ddl_Estado = new List<KeyValuePair<int, string>>();
                                                ddl_Estado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(1, "Sin Asignar"));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(2, "Asignado a Especialista"));
                                                SelectList selectEstado = new SelectList(ddl_Estado, "key", "value", 0);
                                            }

                                            @Html.DropDownList("estadoFiltro", selectEstado, new { style = "width:60%" })
                                        </div>
                                    </div>

                                </div>

                                <div id="conSGD" class="row" style="margin-bottom:10px; display:none">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. SGD(CUT) :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", })
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Estado :</label>
                                        </div>
                                        <div>
                                            @{List<KeyValuePair<int, string>> ddl_EstadoSGD = new List<KeyValuePair<int, string>>();
                                                ddl_Estado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(1, "Sin Asignar"));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(2, "Asignado a Especialista"));
                                                SelectList selectEstadoSGD = new SelectList(ddl_Estado, "key", "value", 0);
                                            }

                                            @Html.DropDownList("estadoFiltroSGD", selectEstado, new { style = "width:60%" })
                                        </div>
                                    </div>

                                </div>


                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Departamento :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDepartamentoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Provincia :</label>
                                        </div>
                                        <div>
                                            <select id="cbxProvinciaFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Distrito :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDistritoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    @*<div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px; display:none">
                                        <div>
                                            <label style="text-align:left;">Fecha de Tramite:</label>
                                        </div>
                                        <div class="form-inline">
                                            del:&nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                            &nbsp;&nbsp; al:&nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>*@
                                      
                                        <div class="col-xs-12  col-md-3" style="text-align:left;">
                                            <div>  &nbsp; </div>
                                            <div>
                                                <button id="btnConsultarPSocios" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>  &nbsp;&nbsp;&nbsp;
                                                <button id="btnLimpiarConsultaPSocios" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                            </div>
                                        </div>

                                        <br />
                                   
                                    </div>

                                <br />
                              </div>
                        </div>

                    </div>
                </div>
                <!-- fin de Acrodeon -->
            </div>
            <!-- fin  pasos-->
        </div>
        <!-- fin contenido-->


        <div class="col-xs-12" style="margin-bottom:25px;">
        <!-- inicio tabla-->
        <div class="table-responsive margen">
            <br />
            <table id="TablaSocios" class="table table-bordered table-hover table-striped display" style="width:100%">
                <thead>
                    <tr>
                        <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">NRO.</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">RUC</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">RAZ. SOCIAL</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">UBICACIÓN</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">NRO. EXPEDIENTE</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">NRO. SGD (CUT)</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">CADENA PRODUCTIVA</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">ESTADO ACTUAL</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">SOCIOS VARONES</th> 
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">SOCIOS DAMAS</th> 
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">TOTAL DE SOCIOS</th> 
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">SOCIOS VARONES PARTICIPAN</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">SOCIOS DAMAS PARTICIPAN</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">TOTAL DE SOCIOS PARTICIPAN</th> 
                        <th colspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                    </tr>
                    <tr> 
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">VER PADRÓN</th> 
                    </tr>
                </thead>
                 
            </table>

        </div>

    </div>
     <!-- FIN TABLA-->

    </div>
     <!-- fin contenido-->

</div>
 <!-- fin contenedor -->



<script type="text/javascript">
    $(document).ready(function () {
        contorles_UP_Socios();

         
    });
     
</script>










