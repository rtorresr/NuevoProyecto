
@{
    ViewBag.Title = "expedientesAsignados";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}

 
<head>
    <title> Expedientes Asignados (Elegibilidad)</title>
</head>
 
<div class="main-content">
<!-- contenedor -->

<div class="main-content-inner">
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">
        <span>Bienvenido  Sr: @Session["NombreUsua"] </span>
        <!-- Fin Ruta -->
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocion/index"> U.Promoción</a></li>
            <li class="active">Expedientes Asignados (Elegibilidad)</li>
        </ul>

    </div><!-- Inicio ruta -->

    <div class="page-content">
        <!-- contenido -->

        <div class="page-header">
            <!-- Inicio titulo -->

            <div class="cbp-mc-column"> 
                <h1> Expedientes Asignados (Elegibilidad) </h1>
            </div>
        </div>      
       <!-- Fin titulo -->
        <!-- inicio pasos-->

        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">

            <!-- contenido de pasos -->
            <!--------- ACORDEON ------------>
            <div id="accordion" style="border:1px solid; ">
                <div class="card">

                    <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Criterios de Búsqueda
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="collapseOne" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <!--------- card body ------------>
                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                        <!-- Inicio Card Body  -->
                        <div class="card-body" style="padding-left:12px;padding-right:12px;">

                            <br />
                            <div class="row" style="margin-bottom:10px;">
                                <div class="col-xs-1 col-sm-2 col-md-3">
                                    &nbsp;
                                </div>
                                <div class="col-xs-10 col-sm-8 col-md-5 panel panel-default">
                                    <div class="panel-body inline-block col-xs-12 col-sm-12 col-md-12" style="text-align:left">
                                        <label class="col-xs-12 col-sm-4 col-md-4"> Buscar Por: </label>
                                        <label class="col-xs-6 col-sm-4 col-md-4"><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                        <label class="col-xs-6 col-sm-4 col-md-4"><input id="rb_sgd" type="radio" name="optradio"> Nro. CUT</label>

                                    </div>
                                </div>
                                <div class="col-xs-1 col-sm-2 col-md-3">
                                    &nbsp;
                                </div>
                            </div>


                            <div id="conRuc" class="row" style="margin-bottom:10px;">
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>RUC :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("perfilUsuar", (string)"", new { @class = "", @MaxLenght = "11", style = "display:none; width:50%", Value = Session["PerfilUsua"] })
                                        @Html.TextBox("idtipoSda", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '1' })
                                        @Html.TextBox("idproceso", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '1' })
                                        @Html.TextBox("idproceso", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '1' })
                                        @Html.TextBox("idtipoIncentivo", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '0' })
                                        @Html.TextBox("idUnidadPcc", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '2' })
                                        @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:50%" })
                                        <button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. Expediente :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("idExpedienteOA", (string)"", new { @class = "", @MaxLenght = "17", style = "display:none; width:50%", disabled = true })
                                        @Html.TextBox("nroExpedienteOA", (string)"", new { @class = "", @MaxLenght = "17", style = "width:60%", disabled = true })
                                        <p id="porprocesoPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde a la linea de Accion PRP. </p>
                                        <p id="porprocesoPRP" style="display:none; font-size:10px; color:darkred">Este expedientecorresponde a la linea de Accion Planes de Negocio. </p>
                                        <p id="porlineaAccionPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de PRP. </p>
                                        <p id="porlineaAccionPRP" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de Planes de Negocio. </p>
                                        @*<select id="cbxExpedientesFl" class="custom-select" style="width:60%;"> </select>*@
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT :</label>
                                    </div>
                                    <div>
                                        <select id="cbxCutExpedientesFl" class="custom-select" style="display:none; width:60%;"> </select>
                                        @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", disabled = true })
                                        <p id="porprocesoPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                        <p id="porprocesoPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                        <p id="porlineaAccionPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                        <p id="porlineaAccionPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Estado :</label>
                                    </div> 
									<div>
										<select id="cbxEstadosFl" class="custom-select"></select>
									</div> 
                                </div>

                            </div>

                            <div id="conSGD" class="row" style="margin-bottom:10px; display:none">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroCutSgd2", (string)"", new { @class = "", style = "width:60%", })
                                        <p id="porprocesoPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                        <p id="porprocesoPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                        <p id="porlineaAccionPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                        <p id="porlineaAccionPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Estado actual:</label>
                                    </div>
                                    <div>
                                        <select id="cbxEstadosFl2" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div> 
                            </div>
                             
                             
                            <div class="row" style="margin-bottom:10px;">
                                
                                <div class="col-xs-12 col-md-6" style="text-align:left">
                                    <div>
                                        <label>Razon Social :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razonSocial", (string)"", new { @class = "campoTexto", style = "width:80%", }) 
                                    </div>
                                </div>
                                
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Oficina Regional :</label>
                                    </div>
                                    <div>
                                        <select id="cbxOficinaRegionalFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>


                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Especialista :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("especialistaAsig", (string)"", new { @class = "", style = "width:90%", })
                                    </div>
                                </div>
                                 
                            </div>


                            <div class="row" style="margin-bottom:10px;">
                                 
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Tipo compromiso :</label>
                                    </div>
                                    <div>
                                        <select id="cbxTipoCompromisoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Compromiso :</label>
                                    </div>
                                    <div>
                                        <select id="cbxCompromisoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                    <div class="inline-block">
                                        <label style="text-align:left;">Fecha de Tramite:</label>
                                        @Html.CheckBox("chkActFechaFl")
                                    </div>
                                    <div class="form-inline"> 
                                        <div class="input-group date" style="width:48%; Text-align:left;">
                                            @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", @placeholder="Desde", disabled = true })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div> 
										&nbsp; 
                                        <div class="input-group date" style="width:48%; Text-align:left;">
                                            @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", @placeholder = "Hasta", disabled = true })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                                 
                            </div>
                              
                            <div class="row" style="margin-bottom:10px;">
                             
                               
                                <div class="col-xs-12 col-md-4" style="text-align:left;">
                                    <div>  &nbsp; </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align:center;">
                                    <div>  &nbsp; </div>
                                    <div>
                                        <button id="btnConsultarExpedienteAsig" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>
                                        <button id="btnLimpiarConsultaExpedienteAsig" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align:left;">
                                    <div>  &nbsp; </div>
                                </div>
                                 
                            </div>

                             
                            <br />
                             
                        </div>
                        <!-- Fn Card Body  -->
                    </div>

                </div>
            </div>

            <!-- fin de Acrodeon -->
            
            <br />
            <br />
            <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                <h3>
                    Lista de Expedientes Asignados
                </h3>
            </div>
            <!-- inicio tabla-->
            <div class="table-responsive margen">
                <table id="tabla_ExpedAsignados" class="table  table-bordered table-hover">
                    <thead>
                        <tr>
                            <th rowspan="1" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro.</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">RUC</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Raz. Social</th> 
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Proceso</th> 
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro. Expediente</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro. CUT</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Compromiso</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Asunto </th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Oficina regional</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Especialista</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha Asignación</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estado Actual</th>
                        </tr>
                          
                    </thead>

                </table>
            </div>
        </div>

    </div><!-- fin contenedor -->
    @*FIN DE TABLA*@

</div>
<!-- fin contenido-->
</div>


<script>
    $(document).ready(function () {
        controles_AsignacionExpedientes();

    }) 
</script>
