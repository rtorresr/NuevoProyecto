
@{
    ViewBag.Title = "solicitudProrroga";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}

<head>
    <title>Solicitud de Prórroga</title>
</head>
<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li class="active">Solicitud de Prórroga</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="cbp-mc-column">

                    <h1> Solicitud de Prórroga</h1>
                </div>
            </div>       <!-- Fin titulo -->
            <!-- inicio pasos ok-->

            <div class="col-xs-12 col-md-12">
                <!-- contenido de ACORDEON 1 -->
                <div id="accordion" style="border:1px solid;">

                    <div class="card">

                        <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-search"></i>
                                    &nbsp; BUSQUEDA SOLICITUDES DE PRÓRROGA
                                </a>
                            </h5>

                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                <br />

                                <div class="row" style="margin-bottom:5px;">
                                    <form class="card-body" style="padding-left:25px;padding-right:25px;">


                                        <div id="conRuc" class="row" style="margin-bottom:25px; margin-left:auto;">

                                            <div class="col-xs-12 col-md-3" style="text-align:left">
                                                <div>
                                                    <label>RUC:</label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })
                                                    <button id="btnCargarDatosRuc" type="button" class="btn btn-info" title="Cargar Datos"> <i class="ace-icon fa fa-search"></i></button>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-md-3" style="text-align:left">
                                                <div>
                                                    <label>Nro. Expediente :</label>
                                                </div>
                                                <div>
                                                    <select id="cbxExpediente" class="custom-select" style="width:60%;"> </select>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-md-3" style="text-align:left">
                                                <div>
                                                    <label>Nro. SGD(CUT):</label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("sgdSolPro", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-md-3" style="text-align:left">
                                                <div>
                                                    <label>Unidad PCC:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxUnidadPCC" class="custom-select" style="width:60%;"> </select>
                                                </div>
                                            </div>

                                        </div>

                                        <!-- columna  3 -->

                                        <div class="row" style="margin-bottom:10px; margin-left: auto;">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                                <div><label style="text-align:left; width:50%">Prórroga:</label></div>
                                                @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                                <div><select id="cbxDepaFr" class="custom-select" style="width:60%;"> </select></div>

                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                                <div><label style="text-align:left; width:50%">Tipo de Incentivo:</label></div>
                                                @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                                <div><select id="cbxTipoInc" class="custom-select" style="width:60%;"> </select></div>

                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left;">
                                                <div>
                                                    <label style="text-align:left;"><strong>Fecha de Registro:</strong></label>
                                                </div>
                                                <div class="form-inline">
                                                    Del:&nbsp;
                                                    <div class="input-group date" style="width:40%; Text-align:left;">
                                                        @Html.TextBox("fecIniPro", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                        <span class="input-group-addon" style="text-align:left; width:5%;">
                                                            <span class="glyphicon glyphicon-calendar"></span>
                                                        </span>
                                                    </div>
                                                    &nbsp;&nbsp; al:&nbsp;
                                                    <div class="input-group date" style="width:40%; Text-align:left;">
                                                        @Html.TextBox("fecFinPro", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                        <span class="input-group-addon" style="text-align:left; width:5%;">
                                                            <span class="glyphicon glyphicon-calendar"></span>
                                                        </span>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>

                                        <div class="row" style="margin-bottom:10px; margin-left: auto;">

                                            <div class="col-xs-12 col-sm-12 col-md-12" style="text-align: center; margin-bottom: 5px; margin-top:25px;">
                                                <button id="btnConsultarSolicitudProrroga" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-search"></i>BUSCAR</button> &nbsp;&nbsp;
                                                <button id="btnLimpiarSolicitudProrroga" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-eraser"></i>LIMPIAR</button>

                                            </div>

                                        </div>


                                    </form>

                                    <!--TABLA SOLICITUD PRORROGA-->
                                    <div class="col-xs-12">
                                        <br />
                                        <h3 style="margin-bottom:20px;"><b>Listado de Solicitud de Prórrogas</b></h3>
                                        <!-- inicio tabla-->
                                        <div class="table-responsive margen">
                                            <table id="tbListaSolProrroga" class="table table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">RUC</th>
                                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">N° Expediente</th>
                                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">N° CUT</th>
                                                        <th rowspan="2" width="15%" style="background-color:darkseagreen; color:black;" class="center">Tipo SDA</th>
                                                        <th rowspan="2" width="8%" style="background-color:darkseagreen; color:black;" class="center">Tipo Incentivo</th>
                                                        <th rowspan="2" width="20%" style="background-color:darkseagreen; color:black;" class="center">Prórroga</th>
                                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Plazo Máximo a Prorrogar</th>
                                                        <th rowspan="2" width="8%" style="background-color:darkseagreen; color:black;" class="center">Fecha de Solicitud</th>
                                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Motivo</th>
                                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Unidad PCC</th>
                                                        <th rowspan="2" width="15%" style="background-color:darkseagreen; color:black;" class="center">Especialista</th>
                                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Correo</th>
                                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Estado</th>
                                                        
                                                        <th rowspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">Acción</th>

                                                    </tr>

                                                </thead>

                                                <tbody>
                                                    <tr>
                                                        <td class="center">1</td>
                                                        <td class="center">Luis Carpio </td>
                                                        <td style="text-align:left"> Arequipa </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td class="center"> 15/10/2015 </td>
                                                        <td class="center"> 22/10/2018 </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td style="text-align:center"> AGRICOLA </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td class="center"> ACTIVO </td>
                                                        <td style="text-align:left"> - </td>
                                                    
                                                        <td class="center">
                                                            <a class="btn btn-primary" id="btnAtenderProrroga" href="#modificarModal" title="Ver" data-toggle="modal">
                                                               Atender
                                                            </a>
                                                        </td>

                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div>

                                    <!--FIN TABLA-->

                                </div>

                               

                            </div>

                        </div><!-- contenido -->

                    </div>

                </div>

            </div>

        </div>

    </div><!-- fin contenedor -->
</div>

<script type="text/javascript">
    $(document).ready(function () {

        eventos_TareasEjec();

    });
</script>
