
@{
    ViewBag.Title = "ReporteTareasEjecutadas";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}

<div class="main-content">
<!-- contenedor -->

<div class="main-content-inner">
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">
        <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
        <!-- Fin Ruta -->
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocion/index"> U.Promocion</a></li>
            <li class="active">Reporte Tareas Ejecutadas</li>
        </ul>

    </div><!-- Inicio ruta -->

    <div class="page-content">
        <!-- contenido -->
    </div>

    <div class="page-header">
        <!-- Inicio titulo -->

        <div class="cbp-mc-column">

            <h1> Reporte Tareas Ejecutadas</h1>
        </div>
    </div>       <!-- Fin titulo -->


    <div class="col-xs-12 col-md-12">

        <div id="accordion" style="border:1px solid;">

            <div class="card">

                <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                    <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <a style="color:white; text-decoration:none;">
                            <i class="ace-icon fa fa-search"></i>
                            &nbsp; BUSQUEDA DE TAREAS EJECUTADAS
                        </a>
                    </h5>
                    <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                            <i class="ace-icon fa fa-chevron-down"></i>
                        </a>
                    </div>
                </div>

                <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                    <div class="card-body" style="padding-left:12px;padding-right:12px;">
                        <br />


                        <!----- VALIDA RUC ----->

                        <div class="row" style="margin-bottom:5px;">
                            <form class="card-body" style="padding-left:25px;padding-right:25px;">

                              
                                <div class="row" style="margin-bottom:10px; margin-left:auto;">
                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Oficina Regional: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            <select id="cbxOfiRegionalFl" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Profesional a Cargo: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            @Html.TextBox("txbProfCargo", (string)" ", new { @class = "campoTexto", @maxlength = "250", style = "width:65%;" })
                                            <p id="txbProfCargo" class="alerta" style="display:none; text-align:right; font-family:'Gill Sans'; color:darkred">Ingresar solo texto en este campo.</p>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Actividad Operativa: &nbsp;&nbsp; </label>
                                        </div>

                                        <div>
                                            <select id="cbxActOperativa" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                </div>

                                <div class="row" style="margin-bottom:10px; margin-left:auto;">
                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Unidad PCC: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            <select id="cbxUnidadPCC" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Tarea: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            <select id="cbxTareaUP" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Tipo de Tarea: &nbsp;&nbsp; </label>
                                        </div>

                                        <div>
                                            <select id="cbxTipoTareaUP" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                </div>

                                
                                <div class="row" style="margin-bottom:10px; margin-left:auto;">
                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Departamento: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            <select id="cbxDepaFl" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Provincia: &nbsp;&nbsp; </label>
                                        </div>
                                        <div>
                                            <select id="cbxProvinciaFl" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 10px;">
                                        <div>
                                            <label style="text-align: left;"> Distrito: &nbsp;&nbsp; </label>
                                        </div>

                                        <div>
                                            <select id="cbxDistritoFl" class="custom-select" style="width:65%; text-align:right;"> </select>
                                        </div>

                                    </div>

                                </div>

                                <div class="row" style="margin-bottom:5px; margin-left:auto;">

                                  <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left; margin-bottom: 5px;">
                                        <div>
                                            <label style="text-align:left;"><strong>Fecha de Registro:</strong></label>
                                        </div>
                                        <div class="form-inline">
                                            Del:&nbsp;
                                            <div class="input-group date" style="width:40%; Text-align:left;">
                                                @Html.TextBox("fecIniTE", (string)"", new { @class = "datetimepicker", style = "width:95%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                            &nbsp;&nbsp; al:&nbsp;
                                            <div class="input-group date" style="width:40%; Text-align:left;">
                                                @Html.TextBox("fecFinTE", (string)"", new { @class = "datetimepicker", style = "width:95%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left; margin-bottom: 5px; margin-top:25px;">
                                        <button id="btnConsultarTareaEjecutada" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-search"></i>BUSCAR</button> &nbsp;&nbsp;
                                        <button id="btnLimpiarTareaEjecutada" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-eraser"></i>LIMPIAR</button>

                                     </div>

                                    </div>

                            </form>
                        </div>



                        @*//diiv contendeor*@
                    </div>
                    <br />

                    <div class="col-xs-12">
                        <br />
                        <h3 style="margin-bottom:20px;"><b>Listado de Tareas Ejecutadas</b></h3>
                        <!-- inicio tabla-->
                        <div class="table-responsive margen">
                            <table id="tbListaTareaEjec" class="table  table-bordered table-hover">
                                <thead>
                                    <tr>
                                        <th rowspan="2" width="3%" style="background-color:darkseagreen; color:black;" class="center">Oficina Regional</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Profesional</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Ubicación</th>
                                        <th rowspan="2" width="15%" style="background-color:darkseagreen; color:black;" class="center">CCPP/Anexo</th>
                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Fecha Inicio</th>
                                        <th rowspan="2" width="20%" style="background-color:darkseagreen; color:black;" class="center">Fecha Termino</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Tarea Relacionada a Unidad</th>
                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Entidad Aliada</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Actividad Operativa</th>
                                        <th rowspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">Tarea</th>
                                        <th rowspan="2" width="20%" style="background-color:darkseagreen; color:black;" class="center">Acción</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Tipo Tarea</th>
                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Descripción Tarea</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Total Participantes</th>
                                        <th rowspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">N° Varones</th>
                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">N° Mujeres</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Resultado de Tarea</th>
                                        <th rowspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">Medio Verificación</th>
                                        <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Recomendación</th>
                                        <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Fecha Registro</th>
                                        <th rowspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">Acuerdo / Compromiso</th>

                                    </tr>
                                  
                                </thead>

                                <tbody>
                                    <tr>
                                        <td class="center">1</td>
                                        <td class="center">Luis Carpio </td>
                                        <td style="text-align:left"> Arequipa </td>
                                        <td class="center"> ACTIVO </td>
                                        <td class="center"> 15/10/2015 </td>
                                        <td class="center"> 22/10/2018 </td>
                                        <td class="center"> ACTIVO </td>
                                        <td style="text-align:center"> AGRICOLA </td>
                                        <td class="center"> ACTIVO </td>
                                        <td class="center"> ACTIVO </td>
                                        <td class="center"> ACTIVO </td>
                                        <td class="center"> ACTIVO </td>
                                        <td style="text-align:left"> - </td>
                                        <td style="text-align:left"> 150 </td>
                                        <td style="text-align:left"> 100 </td>
                                        <td style="text-align:left"> 50 </td>
                                        <td style="text-align:left"> - </td>
                                        <td style="text-align:left"> - </td>
                                        <td style="text-align:left"> - </td>
                                        <td style="text-align:left"> 14/11/2018 </td>
                                        <td class="center">
                                            <a class="btn btn-primary" id="btnVerAcuerdo" href="#modificarModal" title="Ver" data-toggle="modal">
                                                <i class="ace-icon fa fa-eye"></i>
                                            </a>
                                        </td>
                                     
                                        
                                    </tr>
                                </tbody>
                            </table>
                        </div>

                    </div>

                    @*FIN DE TABLA*@

                </div>
            </div>

            <!---- Fin Datos del Contacto ----->
        </div>


    </div>

</div>
</div> <!--CONTENEDOR FINAL-->


<!-- INICIO MODAL ACUERDO COMPROMISO-->

<div class="modal" id="modalAcuerdoCompromiso" tabindex="-1" role="dialog" aria-labelledby="recepcionarLabel" aria-hidden="true" style="overflow-y: auto;">
    <div class="modal-dialog modal-lg">
        <div class="modal-content" style="width:150%; margin-left:-25%;">
            <div class="modal-header">
                <button id="btnCloseAcuerdoComp" type="button" class="close" data-dismiss="modal">x</button>
                <h4 class="modal-title"><strong>ACUERDO / COMPROMISO</strong></h4>
            </div>

            <div class="modal-body" style="max-height:150%">

                <!--Tbla acuerdo-->

                <h3 style="text-align:left; margin-bottom:25px; margin-left:15px;"><b>ACUERDO / COMPROMISO RELACIONADO DIRECTAMENTE CON AGROIDEAS</b></h3>
                <div class="table-responsive margen">
                    <table id="tab_dae" class="table table-bordered table-hover">
                        <thead>
                            <tr>

                                <th width="1%" style="background-color:darkseagreen; color:black;" class="center">N°</th>
                                <th width="17%" style="background-color:darkseagreen; color:black;" class="center">Descripción del Acuerdo / Compromiso</th>
                                <th width="4%" style="background-color:darkseagreen; color:black;" class="center">Entidad Responsable</th>
                                <th width="3%" style="background-color:darkseagreen; color:black;" class="center">Plazo de Cumplimiento</th>
                                
                            </tr>
                        </thead>

                        <tbody>
                            <tr>
                                <td class="center">Activo</td>
                                <td class="center">15/10/2019 </td>
                                <td style="text-align:center"> Robert Rayco </td>
                                <td class="center"> 05 </td>
                               
                            </tr>

                        </tbody>
                    </table>

                    <div class="cbp-mc-column ">
                        <br>

                        <button id="btnSalirAcuerdo" class="btn btn-danger" style="margin-bottom:25px"><i class="ace-icon fa fa-close"></i>Cerrar</button>
                    </div>
                </div>

            </div>
        </div>
    </div>
</div> 


<!-- FIN MODAL ACUERDO COMPROMISO-->


