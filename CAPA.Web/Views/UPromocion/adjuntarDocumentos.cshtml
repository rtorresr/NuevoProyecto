@{
    ViewBag.Title = "AdjuntarDocumentos";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}
 
<title>Adjuntar Documentos</title>
 
 
<div class="main-content">
    <!-- contenedor -->
    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <!-- INICIO RUTA -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li class="active"><a href="/UPromocion/PadronSocios">Adjuntar Documentos</a></li>
            </ul>
        </div><!-- FIN RUTA -->
        <div class="page-content">
            <!-- contenido -->
            <div class="page-header">
                <!-- Inicio titulo -->
                <div class="cbp-mc-column">
                    <h1> Adjuntar Documentos </h1>
                </div>
            </div>
            <!-- Fin titulo -->
            <!-- inicio pasos-->

            <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">

                <!-- contenido de pasos -->
                <!--------- ACORDEON ------------>
                <div id="accordion" style="border:1px solid; ">
                    <div class="card">
                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-search"></i>
                                    Criterios de Búsqueda
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="collapseOne" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">

                                <br />
                                <div class="row" style="margin-bottom:10px;">
                                    <div class="col-xs-1 col-sm-2 col-md-3">
                                        &nbsp;
                                    </div>
                                    <div class="col-xs-10 col-sm-8 col-md-5 panel panel-default">
                                        <div class="panel-body inline-block col-xs-12 col-sm-12 col-md-12" style="text-align:left">
                                            <label class="col-xs-12 col-sm-4 col-md-4"> Buscar Por: </label>
                                            <label class="col-xs-6 col-sm-4 col-md-4"><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                            <label class="col-xs-6 col-sm-4 col-md-4"><input id="rb_sgd" type="radio" name="optradio"> Nro. CUT</label>

                                        </div>
                                    </div>
                                    <div class="col-xs-1 col-sm-2 col-md-3">
                                        &nbsp;
                                    </div>
                                </div>



                                <div id="conRuc" class="row" style="margin-bottom:10px;">
                                    <div class="col-xs-12 col-sm-6 col-md-4" style="text-align:left">
                                        <div>
                                            <label>RUC :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("idUsuario", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = Session["IdUsua"] })
                                            @Html.TextBox("idtipoSda", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '1' })
                                            @Html.TextBox("idproceso", (string)"", new { @class = "campoNumero", style = "display:none; width:50%", Value = '1' })
                                            @Html.TextBox("idtipoIncentivo", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '0' })
                                            @Html.TextBox("idUnidadPcc", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '2' })
                                            
                                            @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero col-xs-12 col-sm-12 col-md-6", @MaxLenght = "11" }) &nbsp;
                                            <button id="btnCargarDatos" class="btn btn-info2" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-4" style="text-align:left">
                                        <div>
                                            <label>Nro. Expediente :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("idExpedienteOA", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", @MaxLenght = "17", style = "display:none;", disabled = true })
                                            @Html.TextBox("nroExpedienteOA", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", disabled = true })
                                            <p id="porprocesoPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP" style="display:none; font-size:10px; color:darkred">Este expedientecorresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de Planes de Negocio. </p>
                                            @*<select id="cbxExpedientesFl" class="custom-select" style="width:60%;"> </select>*@
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-6 col-md-4" style="text-align:left">
                                        <div>
                                            <label>Nro. CUT :</label>
                                        </div>
                                        <div>
                                            <select id="cbxCutExpedientesFl" class="custom-select" style="display:none; width:60%;"> </select>
                                            @Html.TextBox("nroCutSgd", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", disabled = true })
                                            <p id="porprocesoPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                        </div>
                                    </div>
                                </div>

                                <div id="conSGD" class="row" style="margin-bottom:10px; display:none">

                                    <div class="col-xs-12 col-md-4" style="text-align:left">
                                        <div>
                                            <label>Nro. CUT :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("nroCutSgd2", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6" })
                                            <p id="porprocesoPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                        </div>
                                    </div>
                                </div>

                                <div class="row" style="margin-bottom:15px;">
                                    <div class="col-xs-12 col-md-8" style="text-align:left">
                                        <div>
                                            <label>Razón Social:</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("razonSocial", (string)"", new { @class="campoTexto col-xs-12 col-sm-12 col-md-12"})
                                        </div>
                                    </div>
                                </div>

                                <div class="row" style="margin-bottom:15px;">
                                    <div class="col-xs-12 col-md-4" style="text-align:left">
                                        <div>&nbsp;&nbsp;&nbsp;</div>
                                    </div>
                                    <div class="col-xs-12  col-md-4" style="text-align:left;">
                                        <div>  &nbsp; </div>
                                        <div>
                                            <button id="btnConsultarDatos" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>  &nbsp;&nbsp;&nbsp;
                                            <button id="btnLimpiarConsultaDatos" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                        </div>
                                    </div>
                                </div>

                                <br />

                            </div>

                        </div>
                    </div>
                </div>
                <!-- fin de Acrodeon -->

                <!--------- ACORDEON DATOS DOCUMENTOS ------------>
                <div id="accordion2" style="border:1px solid; ">
                    <div class="card">
                        <div class="card-header widget-header" id="headingTwo" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                <a style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Datos del Documento
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                <a href="collapseTwo" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                <br />

                               
                                    <div class="row" style="margin-bottom:5px;">


                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>RUC :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("idDocumentoAdjuntoOA", (string)"", new { @class = "campoNumero col-xs-12 col-sm-12 col-md-6", style = "display:none", disabled = true })
                                                @Html.TextBox("ruc", (string)"", new { @class = "campoNumero col-xs-12 col-sm-12 col-md-6", @MaxLenght = "11", disabled = true })
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Nro. Expediente :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("nroExpediente", (string)"", new { @class = "campoNumero col-xs-12 col-sm-12 col-md-8", @MaxLenght = "11", disabled = true })
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Nro. de CUT :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("idCutExpediente", (string)"", new { style = "display:none", disabled = true })
                                                @Html.TextBox("nroCut", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-8", disabled = true })
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:5px;">
                                        <div class="col-xs-12 col-md-12" style="text-align:left">
                                            <div>
                                                <label>Razón Social :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("razSocial", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-10", disabled = true })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row" style="margin-bottom:5px;">

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Especialista asignado :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("idEspecialista", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-10", style = "display:none", disabled = true })
                                                @Html.TextBox("especialistaAsignado", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-10", disabled = true })
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Unidad PCC :</label>
                                            </div>
                                            <div>
                                                <select id="cbxUnidadProgFr" class="custom-select col-xs-12 col-sm-12 col-md-6"> </select>
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Linea de acción :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("tipoSDA", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", disabled = true })
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:5px;">

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Proceso :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("proceso", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", disabled = true })
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Estado Actual:</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("idEstadoActual", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", style = "display:none", disabled = true })
                                                @Html.TextBox("estadoActual", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-6", disabled = true })
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Registrado por:</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("registradoPor", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-8", disabled = true })
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row" style="margin-bottom:5px;">
                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Oficina Regional :</label>
                                            </div>
                                            <div>
                                                <select id="cbxOficinaRegionalFr" class="custom-select col-xs-12 col-sm-12 col-md-6"> </select>
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Tipo Documento :</label>
                                            </div>
                                            <div>
                                                <select id="cbxTipoDocumentoFr" class="custom-select col-xs-12 col-sm-12 col-md-6"> </select>
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Nro. Documento Adjunto:</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("nroDocAdjunto", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-8" })
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:5px;">
                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Asunto :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("asuntoDoc", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-10" })
                                                <p id="AasuntoDoc" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Referencia :</label>
                                            </div>
                                            <div>
                                                @Html.TextBox("referenciaDoc", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-6" })
                                                <p id="AreferenciaDoc" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                            </div>
                                        </div>

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Fecha Documento:</label>
                                            </div>
                                            <div class="form-inline">
                                                <div class="input-group date" style="width:50%; Text-align:left;">
                                                    @Html.TextBox("fechaDocumento", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                    <span class="input-group-addon" style="text-align:left; width:5%;">
                                                        <span class="glyphicon glyphicon-calendar"></span>
                                                    </span>
                                                </div>
                                            </div>

                                        </div>

                                    </div>

                                    <div class="row" style="margin-bottom:5px;"> 

                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Adjuntar Documento:</label>
                                            </div>

                                            <div class="inline-block">
                                                <div class="row">
                                                    <div class="col-xs-12 col-sm-12 col-md-12">
                                                        <div class="form-group">
                                                            <input id="documento" type="file" name="file" accept="application/pdf" style="margin-bottom:5px" />
                                                        </div>
                                                    </div>
                                                    @Html.TextBox("rutaDoc", (string)"", new { style = "display:none" })
                                                </div>
                                            </div> 
                                        </div>


                                        <div class="col-xs-12 col-md-4" style="text-align:left">
                                            <div>
                                                <label>Grupo que Visualizará el documento: </label>
                                            </div>

                                            <div>
                                                <select id="cbxGrupoVizualizarFr" class="custom-select col-xs-12 col-sm-12 col-md-6"> </select>
                                            </div>

                                        </div>


                                        <div class="col-xs-12  col-md-4" style="text-align:left;">
                                            <div>  &nbsp; </div>
                                            <div>
                                                <button id="btnGrabarDocumentoAdj"  class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-save"></i>  Grabar  </button>  &nbsp;&nbsp;&nbsp;
                                                <button id="btnCancelarDocumentoAdj" class="btn btn-danger" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Cancelar  </button>
                                            </div>
                                        </div>
                                    </div>
                               

                                <br />


                            </div>
                        </div>

                        <br />


                    </div>
                </div>
            </div>
        </div>
        <!-- fin de Acrodeon -->



        <br />
    </div>


    <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
        <h3>
            Lista de documentos Adjuntos
        </h3>
    </div>
    <!-- inicio tabla-->
    <div class="table-responsive margen">
        <table id="tablaDocAdjuntosOA" class="table  table-bordered table-hover">
            <thead>
                <tr>
                    <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">N°</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Unidad Pcc</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Oficina Regional</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Tipo Documento</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Asunto</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Referencia</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">N° de Documento</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha Doc. Adjunto</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estado Actual</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha Registro</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Grupo que Visualiza</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Revisado por OA</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha revisión por OA</th>
                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Documento Adjunto</th>
                    <th colspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                </tr>
                <tr>
                    <th style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Modificar</th>
                    <th style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Eliminar</th>
                </tr>

            </thead>

        </table>
    </div>
</div>

            <!-- fin  pasos-->
        </div>
        <!-- fin contenido-->
              
    </div>
</div>

