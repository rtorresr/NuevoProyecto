@model SEL_Entidades.SEL_E.OA_E

@{
    ViewBag.Title = "verJuntaDirectiva";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}

<head>
    <title>Junta Directiva de la Organización </title>
</head>


<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
            <!-- Inicio Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/UPromocion/"> </a>
                </li>
                <li><a href="/UPromocion/Index">U. Promocion</a></li>
                <li class="active"><a href="verJuntaDirectiva">Junta Directiva de la Organización</a></li>
            </ul>

        </div><!-- Fin ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="cbp-mc-column">
                    <h1> Junta Directiva de la Organización </h1>
                </div>
            </div>       <!-- Fin titulo -->
            <!-- inicio pasos-->



            <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">

                <!--------- ACORDEON ------------>
                <div id="accordion" style="border:1px solid;">
                    <div class="card">

                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Junta Directiva la Organización
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                <br />

                                <div class="row" style="margin-bottom:5px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            <label style="text-align:left;">  RUC : </label>
                                        </div>
                                        <div>
                                            @Html.TextBox("idUsuario", (string)"", new { style = "display:none; width:30%", Value = Session["IdUsua"] })
                                            @Html.TextBox("idUnidadPcc", (string)"", new { style = "display:none; width:30%", Value = "2" })
                                            @Html.TextBox("idJuntaDirectiva", (string)"", new { style = "width:40%; display:none;" })
                                            @Html.TextBoxFor(m=>m.idOA, new { style = "display:none;" }) 
                                            @Html.TextBoxFor(m=>m.rucOA, new {@Class="col-xs-12 col-sm-12 col-md-8", disabled = true})
                                        </div>
                                    </div>


                                    <div class="col-xs-12 col-md-6" style="text-align:left;">
                                        <div>
                                            <label style="text-align:left;"> Razón Social : </label>
                                        </div>
                                        <div>
                                            @Html.TextBoxFor(m=>m.razonSocial, new {@Class="col-xs-12 col-sm-12 col-md-12" , disabled = true })
                                        </div>
                                    </div> 
                                </div>

                                <div class="row" style="margin-bottom:5px;">

                                    <div class="col-xs-12 col-md-3" style="text-align: left;">
                                        <div>
                                            <label style="text-align:left;">Fecha de Inscripción SUNARP : </label>
                                        </div>
                                        <div class="input-group" style="width:50%">
                                            @Html.TextBox("fechaRegistroSunarp", (string)"", new { @Class = "col-xs-12 col-sm-12 col-md-10 datetimepicker", style = "width:100%", disabled = true })
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar" style="text-align:left;"></span>
                                            </span>
                                        </div>
                                    </div>


                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            <label style="text-align: left;"> Fecha de Inicio : </label>
                                        </div>
                                        <div class="input-group date" style="width: 50%;">
                                            @Html.TextBox("fechInicio", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-12 datetimepicker", style = "width:100%; Text_align:left;", disabled = true })
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar" style="text-align:left;"></span>
                                            </span>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            <label style="text-align: left;"> Fecha Fin : </label>
                                        </div>
                                        <div class="input-group date" style="width: 50%;">
                                            @Html.TextBox("fechFin", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-10 datetimepicker", style = "width:100%; Text_align:left;", disabled = true })
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar" style="text-align:left;"></span>
                                            </span>
                                        </div>
                                    </div>


                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            <label style="text-align: left;"> Periodo Duración : </label>
                                        </div>
                                        <div>
                                            @Html.TextBox("periodoDuracion", (string)"", new { @class = "campoNumero", disabled = true })
                                            <label style="text-align:left;"> meses. </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="row" style="margin-bottom:5px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            <label style="text-align:left;">Fecha de registro en Sel : </label>
                                        </div>
                                        <div class="input-group" style="width:50%">
                                            @Html.TextBox("fechaRegistroSel", (string)"", new { @class = "col-xs-12 col-md-4 datetimepicker", @MaxLength = 10, style = "width:100%", disabled = true })
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar" style="text-align:left;"></span>
                                            </span>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-6" style="text-align:left;">
                                        <div>
                                            <label style="text-align:left;">Motivo de Actualización : </label>
                                        </div>
                                        <div>
                                            @Html.TextArea("motivoActualizacion", (string)"", new { @class = "col-xs-12 col-sm-12 col-md-12", @maxlength=350, disabled = true, rows=2, cols=80 })
                                             
                                        </div>
                                    </div>


                                    <div class="col-xs-12 col-md-3" style="text-align:left;">
                                        <div>
                                            &nbsp;&nbsp;&nbsp;
                                        </div>
                                        <div>
                                            <button id="btnRegistroJuntaDirectUP" class="btn btn-success" style="vertical-align:middle;"><i class="fa fa-plus-square"></i> Crear Junta Directiva </button>
                                            <button id="btnModificarJuntaDirecUP" class="btn btn-warning" style="vertical-align:middle; display:none;"><i class="ace-icon fa fa-edit"></i> Modificar Junta Directiva </button>
                                            <button id="btnGrabarJuntaDirecUP" class="btn btn-success" style="vertical-align:middle; display:none;"><i class="ace-icon fa fa-edit"></i> Grabar </button>
                                            <button id="btnCancelarJuntaDirecUP" class="btn btn-danger" style="vertical-align:middle; display:none;"><i class="ace-icon fa fa-edit"></i> Cancelar </button>
                                        </div>
                                    </div>
                                </div>

                                <br />
                            </div>
                        </div>
                        <!-- fin de card body -->
                    </div>
                    <!-- fin de card -->
                </div>
                <!-- fin de Acrodeon -->
            </div>

 

        <!--------- Agregrar Miembros ---------> 
        <div class="row" style="margin-bottom:25px;">
            <br />
            <div class="col-xs-2 col-sm-2 col-md-3">
                &nbsp; &nbsp;
            </div>
            <div class="col-xs-8 col-sm-8 col-md-6">
                <div class="col-xs-12 col-sm-6 col-sm-6">
                    <button id="btnRegistrarMiembroJDUP" class="btn btn-primary col-xs-12 col-sm-8 col-md-10">
                        <i class="ace-icon fa fa-user-plus"></i>AGREGAR MIEMBRO
                    </button>
                </div>

                <div class="col-xs-12 col-sm-6 col-sm-6">
                    <button id="btnSalirJuntaDirectivaUP" class="btn btn-danger col-xs-12 col-sm-8 col-md-10">
                        <i class="glyphicon glyphicon-undo"></i> &nbsp; SALIR 
                    </button>
                </div>
            </div>
            <div class="col-xs-1 col-sm-1 col-md-3">
                &nbsp; &nbsp;
            </div>
        </div>




        <br />

        <!-- inicio Tabla Junta -->
        <div class="col-xs-12" style="margin-bottom:25px;">

            <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                <h3>
                    Lista de Miembros de la junta directiva de la organización agropecuaria
                </h3>
            </div>


            <!-- inicio tabla-->
            <div class=" table-responsive margen">
                <table id="tablaMiembroJD" class="table  table-bordered table-hover">
                    <thead>
                        <tr>
                            <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Nro.</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">DNI</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Nombre Completo </th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Cargo</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Pertenece a la OA</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Correo Eléctronico</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Teléfono</th>
                            <th colspan="3" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                        </tr>
                        <tr>
                            <th class="text-center" style="background-color:darkseagreen; color:black; min-width:2%; max-width:2%">Modificar</th>
                            <th class="text-center" style="background-color:darkseagreen; color:black; min-width:2%; max-width:2%">Eliminar</th>
                        </tr>
                    </thead>


                </table>

                <div class="col-md-12 text-center">
                    <ul class="pagination pagination-lg pager" id="developer_page"></ul>
                </div>
            </div>

        </div> <!-- Fin tabla Junta  -->

    </div><!-- contenido -->
</div><!-- fin contenedor -->


<script>
    $(document).ready(function () {
        controles_Juntadirectiva();

    });
</script>

