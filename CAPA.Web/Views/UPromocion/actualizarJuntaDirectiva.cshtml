@{
    ViewBag.Title = "actualizarJuntaDirectiva";
    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}
<head>
    <title>Actualizar Junta Directiva</title>  
</head>
<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li class="active">Actualizar Junta Directiva</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="col-md-6" style="text-align:left">

                    <h1> Actualizar  Junta Directiva de la Organización </h1>
                </div>
            </div>       
            <!-- Fin titulo -->

            <!-- inicio pasos--> 
            <div class="col-xs-12 col-md-12" style="margin-bottom:25px;"> 
                <!-- contenido de pasos -->

                <!--------- ACORDEON ------------> 
                <div id="accordion" style="border:1px solid; ">
                    <div class="card">

                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-search"></i>
                                    Criterios de Búsqueda
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
 
                                <br />
                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>RUC :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:50%" })
                                            <button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-6" style="text-align:left">
                                        <div>
                                            <label>Razon Social  :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("razonSocial", (string)"", new { @class = "", style = "width:80%", disabled = true })
                                        </div>
                                    </div>
 
                                    <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                        <div>
                                            <label style="text-align:left;">Periodo Vigencia:</label>
                                        </div>
                                        <div class="form-inline">
                                            desde &nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                            &nbsp;&nbsp; hasta:&nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                     
                                </div>

                             

                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Departamento :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDepartamentoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Provincia :</label>
                                        </div>
                                        <div>
                                            <select id="cbxProvinciaFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Distrito :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDistritoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>
                                     
                                    <div class="col-xs-12  col-md-3" style="text-align:left;">
                                        <div>  &nbsp; </div>
                                        <div>
                                            <button id="btnConsultarJDirectiva" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>  &nbsp;&nbsp;&nbsp;
                                            <button id="btnLimpiarConsultaJDirectiva" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                        </div>
                                    </div>
                                     
                                </div>
                                 
                                <br />

                            </div>
                        </div>

                    </div>
                </div>
                <!-- fin de Acrodeon -->
            </div>

            <!-- FIN pasos-->
              

            <div class="col-xs-12" style="margin-bottom:25px;">
                <!-- inicio tabla-->
                <div class="table-responsive margen">
                    <br />
                    <table id="TablaJDirec" class="table table-bordered table-hover table-striped display" style="width:100%">
                        <thead>
                            <tr>
                                <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">NRO.</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">RUC</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">RAZ. SOCIAL</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">UBICACIÓN</th> 
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">REPRESENTANTE LEGAL</th> 
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">FECHA INICIO</th> 
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:15%">FECHA FIN</th> 
                                <th colspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                            </tr>
                            
                        </thead>

                    </table>

                </div>

            </div>
            <!-- FIN TABLA-->


            <div id="recepcionModal" class="modal fade">
                <div class="modal-dialog">
                    <div class="modal-content">
                        <div class="modal-header alert-success alert-dismissible">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                            <h4 class="modal-title">Recepcionar Expediente</h4>
                        </div>
                        <div class="modal-body">
                            <p>Datos Generales de la Organizacion</p>
                            <hr />
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">R.U.C :</label></div>
                                <div class="col-xs-8" style="text-align:left;">@Html.TextBox("nro_orden", (string)"20570607945", new { style = "width:100%", disabled = "true" })</div>
                            </div>
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;"><label style=" text-align:left;">Razón Social :</label></div>
                                <div class="col-xs-8">@Html.TextBox("nro_orden", (string)"ASOC. DE AGRIC. AGROPEC. 'RAYOS DEL DIAMANTE'", new { style = "width:100%", disabled = "true" })</div>
                            </div>
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;"><label style=" text-align:left;">Domicilio Legal</label></div>
                                <div class="col-xs-8">@Html.TextBox("nro_orden", (string)"CAJAMARCA / SAN IGNACIO / SAN JOSE DE LOURDES", new { style = "width:100%", disabled = "true" })</div>
                            </div>
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">Representante Legal</label></div>
                                <div class="col-xs-8">@Html.TextBox("nro_orden", (string)"JUAN PEREZ", new { style = "width:100%", disabled = "true" })</div>
                            </div>
                            <div class="row" style="margin-bottom:10px;">
                                <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">Persona de Contacto</label></div>
                                <div class="col-xs-8">@Html.TextBox("nro_orden", (string)"PEDRO LOPEZ", new { style = "width:100%;", disabled = "true" })</div>
                            </div>

                            <p>Datos para la Recepción</p>
                            <hr />
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">Origen del Expediente</label></div>
                                <div class="col-xs-8">
                                    @{
                                        List<KeyValuePair<int, string>> dropdownList9 = new List<KeyValuePair<int, string>>();
                                        dropdownList9.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                        dropdownList9.Add(new KeyValuePair<int, string>(1, "Arequipa"));
                                        dropdownList9.Add(new KeyValuePair<int, string>(2, "Apurimac"));
                                        dropdownList9.Add(new KeyValuePair<int, string>(2, "Lima"));
                                        dropdownList9.Add(new KeyValuePair<int, string>(2, "Ica"));
                                        SelectList selectList9 = new SelectList(dropdownList9, "key", "value", 0);

                                    }
                                    @Html.DropDownList("foo", selectList9, new { style = "width:100%" })
                                </div>
                            </div>
                            <div class="row" style="margin-bottom:5px;">
                                <div class="col-xs-4" style="text-align:left;">Fecha de Expediente</div>
                                <div class="col-xs-8">
                                    <div class="form-group">
                                        <div class='input-group date' id='datetimepicker2'>
                                            @Html.TextBox("nro_orden", (string)ViewBag.bus_num, new { style = "width:100%" })
                                            <span class="input-group-addon">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-success" data-dismiss="modal">Actualizar Junta</button>
                            <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
             


        </div>
        
        
        <!-- contenido -->
    </div>

</div><!-- fin contenedor -->

<script type="text/javascript">
    
</script>










