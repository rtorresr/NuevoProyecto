@{

    Layout = "~/Views/Shared/_LayoutMenuUp.cshtml";
}

<head>
    <title>Detalle Actividad</title>
 </head>
<style>

</style>
<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li><a href="/UPromocion/consultarActividades">Ver Actividad<</a></li>
                <li class="active">Detalle Actividad</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="col-md-6" style="text-align:left">

                    <h1> Detalle Actividad</h1>
                </div>
            </div>       <!-- Fin titulo -->
            <!-- inicio pasos-->

            <div class="col-xs-12">
                <!-- contenido de pasos -->
                <div class="row" style="margin-bottom:25px;">
                    <br />
                    <a class="btn btn-success" id="" href="/UPromocion/consultarActividades" title="Imprimir">
                        <i class="ace-icon fa fa-print"></i>IMPRIMIR
                    </a>

                    <a class="btn btn-success" id="" href="/UPromocion/consultarActividades" title="Cancelar">
                        <i class="ace-icon fa fa-undo"></i>CANCELAR
                    </a>

                </div>
                <div class="row">
                    <div class="col-xs-12" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                        <h3>
                            <i class="ace-icon fa fa-file"></i>
                            Datos del Especialista

                        </h3>
                    </div>

                </div>
                <div class="row" style="margin-bottom:25px;">
                    <div class="col-md-4" style="text-align:left;">

                        <label style=" text-align:left;">
                            Unidad Regional :
                        </label>

                        @Html.TextBox("nro_orden", (string)"UNIDAD REGIONAL AREQUIPA", new { @class = "solo-numero", style = "width:60%", disabled = "true" })

                    </div>
                    <div class="col-md-4" style="text-align:left;">
                        <label style=" text-align:left;">
                            Nombre del Especialista :
                        </label>
                        @Html.TextBox("razon_social", (string)"JORGE AUGUSTO, AMAYA CASTILLO", new { @MaxLength = "500", style = "width:60%", disabled = "true" })

                    </div>
                    <div class="col-md-4" style="text-align:left;">

                        <label style=" text-align:left;">
                            Cargo :
                        </label>

                        @Html.TextBox("nro_orden", (string)"JEFE DEL PROGRAMA", new { @class = "solo-numero", style = "width:60%", disabled = "true" })

                    </div>
                </div>

                <br />


                <div id="accordion" style="border:1px solid;">
                    <div class="card">
                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Datos de la Actividad Programada
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;">
                                <br />
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-6" style="text-align:left; border-bottom:1px solid #ede9e9;">

                                        <label style=" text-align:left;">
                                            Aliados
                                        </label>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Aliados :
                                        </label>
                                      

                                    </div>
                                    <div class="col-md-4" style="text-align:left;" id="aliado">
                                        @Html.TextBox("nro_orden", (string)"---", new { style = "width:100%", disabled = "true" })
                                       
                                    </div>
                                    
                                </div>

                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-6" style="text-align:left; border-bottom:1px solid #ede9e9;">

                                        <label style=" text-align:left;">
                                            Organización
                                        </label>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Organizacion :
                                        </label>
                                        
                                    </div>
                                    <div class="col-md-4" style="text-align:left;" id="organizacion">
                                       @Html.TextBox("nro_orden", (string)"ORGANIZACION DE PRODUCTORES AGRARIOS ABC DE CACAO", new { style = "width:100%", disabled = "true" })

                                    </div>
                                    
                                </div>

                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-6" style="text-align:left; border-bottom:1px solid #ede9e9;">

                                        <label style=" text-align:left;">
                                            Actividad
                                        </label>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Actividad a programarse :
                                        </label>
                                    </div>
                                    <div class="col-md-4" style="text-align:left;" id="organizacion">

                                       @Html.TextBox("nro_orden", (string)"CAPACITACION", new { style = "width:100%", disabled = "true" })

                                    </div>

                                </div>

                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">
                                        <label style=" text-align:left;">
                                            Fecha de la Actividad a programarse :
                                        </label>
                                    </div>
                                    <div class="col-md-3" style="text-align:left;">
                                       @Html.TextBox("nro_orden", (string)"17/08/2018", new { style = "width:40%;", disabled = "true" })
                                    </div>
                                    <div class="col-md-1" style="text-align:left;">
                                        <label style=" text-align:left;">
                                            Hora de Inicio :
                                        </label>
                                    </div>
                                    <div class="col-md-2" style="text-align:left;">
                                        @Html.TextBox("nro_orden", (string)"08:15", new { style = "width:40%;", disabled = "true" })
                                    </div>
                                    <div class="col-md-1" style="text-align:left;">
                                        <label style=" text-align:left;">
                                            Hora de Fin :
                                        </label>
                                    </div>
                                    <div class="col-md-2" style="text-align:left;">
                                        @Html.TextBox("nro_orden", (string)"15:30", new { style = "width:40%;", disabled = "true" })
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">
                                        <label style=" text-align:left;">
                                            Descripción de la Actividad :
                                        </label>
                                    </div>
                                    <div class="col-md-8" style="text-align:left;">
                                      @Html.TextArea("nro_orden", (string)"CAPACITACION A ORGANIZACION DE PRODUCTORES AGRARIOS ABC DE CACAO EN LA OFICINA REGIONAL", new { @rows = "3", style = "width:100%; resize:none;", disabled = "true" })
                                    </div>
                                </div>



                            </div>
                        </div>
                    </div>

                    <div class="card">
                        <div class="card-header widget-header" id="headingTwo" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Datos del Resultado de la Actividad Programada
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;">
                                <br />
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-6" style="text-align:left; border-bottom:1px solid #ede9e9;">

                                        <label style=" text-align:left;">
                                           Resultados
                                        </label>
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Resultado de la Actividad :
                                        </label>


                                    </div>
                                    <div class="col-md-4" style="text-align:left;" id="aliado">
                                        @Html.TextBox("nro_orden", (string)"REALIZADO", new { style = "width:40%", disabled = "true" })

                                    </div>

                                </div>

                                
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Fecha de Realización :
                                        </label>

                                    </div>
                                    <div class="col-md-4" style="text-align:left;" id="organizacion" >
                                        @Html.TextBox("nro_orden", (string)"17/08/2018", new { style = "width:40%;", disabled = "true" })

                                    </div>

                                </div>

                               
                                <div class="row" style="margin-bottom:25px;">
                                    <div class="col-md-2" style="text-align:left;">

                                        <label style=" text-align:left;">
                                            Descripción del Resultado :
                                        </label>
                                    </div>
                                    <div class="col-md-8" style="text-align:left;" id="organizacion">

                                        @Html.TextArea("nro_orden", (string)"SE RALIZO CON EXITO LA CAPACITACION A ORGANIZACION DE PRODUCTORES AGRARIOS ABC DE CACAO EN LA OFICINA REGIONAL", new { @rows = "3", style = "width:100%; resize:none;", disabled = "true" })
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>


                </div>

                <!-- -->
            </div>
            <div class="row">
                <!-- inicio grid-->
                <!-- Fin tabla-->


            </div><!-- fin grid -->
        </div><!-- contenido -->
    </div>

</div><!-- fin contenedor -->

<script>


</script>
