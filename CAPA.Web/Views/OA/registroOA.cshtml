@{
    ViewBag.Title = "registroOA";
    Layout = "~/Views/Shared/_LayoutMenuOA.cshtml";
}

<head>
    <title>Registro de Organizaciones de Productores Agrarios</title>
</head>


<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
