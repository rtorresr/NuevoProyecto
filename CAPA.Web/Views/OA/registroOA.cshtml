                <!-- Inicio Ruta -->
                <ul class="breadcrumb">
                    <li>
                        <i class="ace-icon fa fa-home home-icon"></i>
                        <a href="/OA/"> </a>
                    </li>
                    <li><a href="/OA/Index">OA</a></li>
                    <li class="active"> <a href="/OA/registroOA">registro de Información de Organizaciones de Productores Agrarios</a></li>
                </ul>

            </div><!-- Fin ruta -->

            <div class="page-content">
                <!-- contenido -->

                <div class="page-header">
                    <!-- Inicio titulo -->

                    <div class="col-md-12">
                        <h1>Registro de Información de Formato Nro 01</h1>
                    </div>
                </div>       <!-- Fin titulo -->
                <!-- inicio pasos-->

                <div class="col-md-12">
                    <!-- contenido de pasos -->
                    <div class="row" style="margin-bottom:25px;">
                        <br />

                        <a id="btnSalir" class="btn btn-success" href="/OA/Index" title="Regresar">
                            <i class="ace-icon fa fa-undo"></i> Regresar
                        </a>

						<div id="content2" class="col-lg-12" style="display:none">
							<img src="~/Content/images/loader.gif" /><br /> Cargando, un momento por favor...
						</div>

                    </div>

                    <br />

					<div class="row">
						
					</div>

					 

                    <div class="col-xs-12 col-md-12">
                        <!--------- ACORDEON 1  card-header ------------>
                        <div id="accordion" style="border:1px solid;">

                            <div class="card">

                                <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                                    <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                        <a style="color:white; text-decoration:none;">
                                            <i class="ace-icon fa fa-file"></i>
                                            &nbsp; Información General de la Organización
                                        </a>
                                    </h5>
                                    <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                        <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                                            <i class="ace-icon fa fa-chevron-down"></i>
                                        </a>
                                    </div>
                                </div>
                                 
                                <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                                    <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                        <br />


                                        <div class="row">
                                            <div class="col-xs-12 col-sm-12 col-md-11" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                                <h3>
                                                    <i class="ace-icon fa fa-file"></i>
                                                    Información General
                                                </h3>
                                            </div>
                                        </div>



                                        <!----- VALIDA RUC ----->
                                        <div class="row" style="margin-bottom:5px;">
                                            @*margin-bottom: 5px;*@
                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom:5px">
                                                <div>
                                                    <label style="text-align:left;">RUC :</label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("idUsuario", (string)"", new { @class = "campoNumero", @MaxLength = "9", style = "display:none; width:65%;", Value = @Session["IdUsua"] }) 
                                                    @Html.TextBox("idOA", (string)"", new { @class = "campoNumero", @maxlength = "8", style = "display:none; width:32%" })
                                                    @Html.TextBox("idTipoSolic", (string)"", new { @class = "campoNumero", @maxlength = "11", style = "display:none; width:32%", disabled = true })
                                                    @Html.TextBox("permitirAct", (string)"", new { style = "display:none" })

                                                    @Html.TextBox("completado", (string)"", new { @class = "campoTexto", @maxlength = "6", style = "display:none; width:32%" })
                                                    @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero col-xs-8 col-sm-6 col-md-5", @maxlength = "11",  disabled = true, Value = @Session["RucOA"] }) &nbsp;
                                                    <button type="button" class="btn btn-info" id="btnConsultaPideSunat" title="Validar"> <i class="ace-icon fa fa-search"></i></button><br />
                                                    <p id="ANroDocIdent" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                                </div>
                                            </div>


                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label style="text-align: left;"> Estado Activo : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("esActivo", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8", disabled = true })
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label style="text-align: left;"> Condición : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("esHabido", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8", disabled = true })
                                                </div>
                                            </div>
                                             
                                        </div>


                                        <div class="row" style="margin-bottom:10px;">
                                            <div class="col-xs-12 col-sm-12 col-md-10" style="text-align: left; margin-bottom:5px">
                                                <div><label style="text-align:left;"> Razón Social : </label></div>
                                                <div>
                                                    @Html.TextBox("razonSocial", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-md-10", @MaxLength = "250", disabled = "disabled" })
                                                    <p id="ARazSocial" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row" style="margin-bottom:10px;"> 

                                            <div id="estCivil" class="col-xs-12 col-sm-12 col-md-4" style="text-align: left; display:none">
                                                <div>
                                                    <label style="text-align: left;"> Estado Civil : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("EstCivil", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-6", @maxlength = "8",   disabled = "true" })
                                                    <p id="AEstCivil" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                                </div>
                                            </div>

                                            <div id="dniCony" class="col-xs-12 col-sm-12 col-md-4" style="text-align: left; display:none;">
                                                <div>
                                                    <label style="text-align: left;"> D.N.I de conyuge : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("Dnicony", (string)"", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-6", @MaxLength = "8" }) &nbsp;
                                                    <button id="btnVerifDniCony" class="btn btn-info2" title="Verificar existencia en SEL"> <ion-icon name="search"></ion-icon></button>
                                                    <p id="ADnicony" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                                </div>
                                            </div>
                                        </div>


                                        <!----- FECHAS ------>
                                        <div class="row" style="margin-bottom:10px;">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; ">

                                                <div>
                                                    <label style="text-align:left;">Tipo Organización :</label>
                                                </div>
                                                <div>
                                                    @Html.Hidden("idTipoOrgan", (string)"", new { @class = "" })
                                                    @Html.TextBox("tipoOrganizacion", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-10", @MaxLength = "250", disabled = true })
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-16 col-md-3" style="text-align: left; margin-bottom:5px">
                                                <div>
                                                    <label style="text-align:left;">Fecha de Inscripción SUNARP : </label>
                                                </div>
                                                <div class="input-group col-xs-12 col-sm-6 col-md-8">
                                                    @Html.TextBox("FecInscSunarp", (string)"", new { @class = "datetimepicker col-xs-12 col-sm-12 col-md-12" })
                                                    <span class="input-group-addon">
                                                        <span class="glyphicon glyphicon-calendar" style="text-align:left;"></span>
                                                    </span>
                                                </div>
                                            </div>



                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label style="text-align:left;">N° de Ficha Registral : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("campofichaRegistro", (string)"", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-8", @MaxLength = 9, @placeholder="0" })
                                                    <p id="AcampofichaRegistro" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                                </div>
                                            </div>
											 
                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label style="text-align: left;">Fecha de Acta de Constitución : </label>
                                                </div>

                                                <div class="input-group col-xs-12 col-sm-8 col-md-8">
                                                    @Html.TextBox("FecActaConstitucion", (string)"", new { @class = "datetimepicker col-xs-12 col-sm-12 col-md-12" })
                                                    <span class="input-group-addon" style="text-align:left;">
                                                        <span class="glyphicon glyphicon-calendar"></span>
                                                    </span>
                                                </div>
                                                <br />
                                            </div>

                                        </div>

          
                                        <!-- --------- Telefonos -------- -->
                                  
                                        <div class="row">
                                            <div class="col-xs-12 col-md-11" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                                <h3 class="form-inline">
                                                    <ion-icon name="call"></ion-icon>
                                                     Teléfono de la Organización
                                                </h3>
                                            </div>
                                        </div>


                                        <div class="row" style="margin-bottom:10px;">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Teléfono fijo :  </label>
                                                </div>
                                                <div> 
                                                    @Html.TextBox("NroTelefOA", (string)"", new { @class = "campoTelefono col-xs-12 col-sm-6 col-md-8", @MaxLength = "9" })
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Anexo  : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("AnexoOA", (string)"", new { @class = "campoTelefono col-xs-12 col-sm-6 col-md-8", @MaxLength = "4"})
                                                </div>
                                            </div>


                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">

                                                <div>
                                                    <label style="text-align: left;"> Teléfono celular (*):  </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("NroTelefOA2", (string)"", new { @class = "campoTelefono col-xs-12 col-sm-6 col-md-8", @MaxLength = "9" })
                                                    <p id="ANroTelefOA2" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                                </div>
                                            </div>


                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Teléfono celular :  </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("NroTelefOA3", (string)"", new { @class = "campoTelefono col-xs-12 col-sm-6 col-md-8", @maxlength = "9"})
                                                    <p id="ANroTelefOA3" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                                </div>
                                            </div>
                                        </div>


                                        <!-- --------- fin Telefonos -------- -->
                                    

                                        <div class="row">
                                            <div class="col-xs-12 col-md-11" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                                <h3 class="form-inline">
                                                    <ion-icon name="home"></ion-icon>
                                                     Dirección Legal de la Organización
                                                </h3>
                                            </div>
                                        </div>


                                        <div class="row" style="margin-bottom: 10px;">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Departamento :  </label>
                                                </div>
                                                <div>
                                                    @*<select id="cbxDepartamentoFr" class="custom-select" style="width:60%;"> </select>*@
                                                    @Html.TextBox("departamento", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8",  disabled = true })
                                                </div>

                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Provincia :  </label>
                                                </div>
                                                <div>
                                                    @*<select id="cbxProvinciaFr" class="custom-select" style="width:60%;"> </select>*@
                                                    @Html.TextBox("provincia", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8", disabled = true })
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Distrito : </label>
                                                </div>
                                                <div>
                                                    @*<select id="cbxDistritoFr" class="custom-select" style="width:60%;"> </select>*@
                                                    @Html.TextBox("distrito", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8", disabled = true })
                                                    @Html.TextBox("codigoUbigeo", (string)" ", new { style = "width:65%; display:none;", disabled = true })
                                                </div>
                                            </div> 
                                        </div>

                                        <div class="row" style="margin-bottom:10px">
                                            <div class="col-xs-12 col-sm-12 col-md-10" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;">Dirección : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("DireccionLegOA", (string)" ", new { @class = "campoGenerico  col-xs-12 col-sm-6 col-md-10", @maxlength = "250", disabled = true })
                                                    &nbsp; &nbsp; @Html.CheckBox("activaDirecLegOA", false, new { disabled = true }) <lable>Desbloquear campo.</lable> 
                                                    <p id="ANroTelefOA3" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="row" style="margin-bottom:10px">
                                            <div class="col-xs-12 col-sm-12 col-md-10" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;">Centro Poblado : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("CentroPoblado", (string)"", new { @class = "campoGenerico col-xs-12 col-sm-6 col-md-10", @maxlength = "250" })
                                                </div>
                                            </div>
                                        </div>
                                                
                                        <div class="row" style="margin-bottom:10px">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;">Valor de Quintil : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("valorQuintilPobreza", (string)"", new { @class = "campoGenerico col-xs-12 col-sm-6 col-md-8", @maxlength = "250", disabled = true })
                                                </div>
                                            </div>
                                             
                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Nivel de Quintil Pobreza:  </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("nivelQuintil", (string)"", new { @class = "campoTexto col-xs-12 col-sm-6 col-md-8", @maxlength = "250", disabled = true })
                                                </div>
                                            </div>


                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;">Altitud por Distrito: </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("altitud", (string)"", new { @class = "campoGenerico col-xs-12 col-sm-6 col-md-8", @maxlength = "250", disabled = true })
                                                </div>
                                            </div>
                                      
                                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                    <div>
                                                        <label style="text-align: left;">Tipo Ambito : </label>
                                                    </div>
                                                    <div>
                                                        @Html.TextBox("idZonaIntervencion", (string)"", new { @class = "campoGenerico", @maxlength = "250", style = "width:65%; display:none" })
                                                        @Html.TextBox("descripAmbito", (string)"", new { @class = "campoGenerico col-xs-12 col-sm-6 col-md-8", @maxlength = "250", disabled = true })
                                                    </div>
                                                </div>
                                            </div>

                                    </div>
                                     


                                    <div class="row">
                                        <div id="Content3" style="display:none">
                                            <img src="~/Content/images/loader.gif" /><br />Un momento, por favor...
                                        </div>
                                    </div>



                                    <div class="row" style="margin-bottom:25px;">
                                        <br />
                                        <button id="btnRegistraOA" class="btn btn-success" style="display:none;" title="Registrar">
                                            <i class="ace-icon fa fa-pencil"></i> REGISTRAR
                                        </button>

                                        <button id="btnModificarOA" class="btn btn-warning" style="display:none;" title="Registrar">
                                            <i class="ace-icon fa fa-pencil"></i> MODIFICAR
                                        </button>

                                        <button id="btnGrabarOA" class="btn btn-info" style="display:none;" title="Grabar">
                                            <i class="ace-icon fa fa-save"></i> GRABAR
                                        </button>

                                        <button id="btnCancelarOA" class="btn btn-danger" style="display:none;" title="Cancelar">
                                            <i class="ace-icon fa fa-undo"></i> CANCELAR
                                        </button> 
                                    </div>

                                </div>
                            </div>

                            <!------- Fin de Acordeon -------->
                            <!----- Fin Datos de la Organización ----->
                            <!--------- ACORDEON 2 ------------>
                            <!----- Datos Representante Legal ----->

                            <div class="card">

                                <div class="card-header widget-header" id="heading" style="text-align:left;">
                                    <h5 class="widget-title" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                        <a style="color:white; text-decoration:none;">
                                            <i class="ace-icon fa fa-file"> </i>
                                            Representante Legal de la Organización
                                        </a>
                                    </h5>
                                    <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseTwo" aria-expanded="true" aria-controls="collapseTwo">
                                        <a href="#collapseTwo" data-action="collapse" style="color:#fff;">
                                            <i class="ace-icon fa fa-chevron-down"></i>
                                        </a>
                                    </div>
                                </div>


                                <div id="collapseTwo" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                                    <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                        <br />


                                        <div class="row">
                                            <div class="col-xs-12 col-md-11" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                                <h3>
                                                    <i class="ace-icon fa fa-file"></i>
                                                    Información del Representante Legal
                                                </h3>
                                            </div>
                                        </div>


                                        <div class="row" style="margin-bottom:10px;">

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> D.N.I : </label>
                                                </div>
                                                <div>

                                                    @Html.Hidden("completadoRL", (string)"", new { @class = "campoNumero", @MaxLength = "8", style = "width:65%;", })
                                                    @Html.Hidden("idRepLegal", (string)"", new { @class = "campoNumero", @MaxLength = "8", style = "width:65%;" })
                                                    @Html.Hidden("usuario", (string)"", new { @class = "campoNumero", @MaxLength = "50", style = "width:65%;", Value = @Session["Usuario"] })
                                                    @Html.Hidden("idOA2R", (string)"", new { @class = "campoNumero", @MaxLength = "50", style = "width:65%;" })
                                                    @Html.Hidden("docUsuario", (string)"", new { @class = "campoNumero", @MaxLength = "8", style = "width:42%;", @Value = Session["DocumentoUsua"] })
                                                    @Html.TextBox("dniRepresentanteLegal", (string)"", new { @class = "campoNumero col-xs-8 col-sm-6 col-md-6", @maxlength = "8", disabled = true }) &nbsp;
                                                    <button class="btn btn-info info2" id="idBuscarRepLeg" href="#" title="Validar"> <i class="ace-icon fa fa-search"></i></button>
                                                    <p id="ANroDocRep" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Nombres : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("NombRepLeg", (string)"", new { @Class = "campoTexto col-xs-12 col-sm-6 col-md-8", @MaxLength = "50", disabled = true })
                                                    <p id="ANombRepLeg" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                                <div>
                                                    <label style="text-align: left;"> Apellido Paterno : </label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("ApelPatRepLeg", (string)"", new { @Class = "campoTexto col-xs-12 col-sm-6 col-md-8", @MaxLength = "50", disabled = true })
                                                    <p id="AApelPatRepLeg" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                                </div>
                                            </div>


