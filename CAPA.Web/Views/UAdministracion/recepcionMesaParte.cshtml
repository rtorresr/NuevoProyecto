
@{
    ViewBag.Title = "recepcionMesaParte";
    Layout = "~/Views/Shared/_LayoutMenuUa.cshtml";
}

<head>
    <title>Mesa de Parte - Organizaciones registradas </title>
</head>

<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/index"> U.Administración</a></li>
                <li class="active">Mesa de Parte</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="cbp-mc-column">

                    <h1> Mesa de Parte - Recepción de Expediente  </h1>
                </div>
            </div>       <!-- Fin titulo -->
            <!-- inicio pasos-->

            <div class="col-xs-12">
                <!-- contenido de pasos -->
                <div id="accordion" style="border:1px solid;">
                    <div class="card">

                        <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Criterios de Búsqueda
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>


                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                <br />

                                <div class="row" style="margin-bottom:5px;">
                                    <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>RUC :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero col-xs-12 col-sm-12 col-md-8", @MaxLenght = "11", })
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col- col-md-6" style="text-align:left">
                                        <div>
                                            <label>Razón Social :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("razSocial", (string)"", new { @class = "campoTexto col-xs-12 col-sm-12 col-8" })
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Estado :</label>
                                        </div>
                                        <div>
                                            @{List<KeyValuePair<int, string>> ddl_Estado = new List<KeyValuePair<int, string>>();
                                                ddl_Estado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(1, "Nuevo"));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(2, "Recepcionado"));
                                                SelectList selectEstado = new SelectList(ddl_Estado, "key", "value", 0);
                                            }
                                            @Html.DropDownList("cbxEstafiltro", selectEstado, new {@class="col-xs-12 col-sm-12 col-md-8" })
                                        </div>
                                    </div>

                                </div>

                                <div class="row" style="margin-bottom:5px;">

                                    <div class="col-xs-12 col-sm-12  col-md-3" style="text-align: left;">
                                        <div>
                                            <label style="text-align:left;">Línea de Acción : </label>
                                        </div>
                                        <div>
                                            <select id="cbxTipoSDAFl" class="custom-select col-xs-12 col-sm-12 col-md-8"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Departamento :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDepartamentoFl" class="custom-select col-xs-12 col-sm-12 col-md-8"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Provincia :</label>
                                        </div>
                                        <div>
                                            <select id="cbxProvinciaFl" class="custom-select  col-xs-12 col-sm-12 col-md-8"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Distrito :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDistritoFl" class="custom-select  col-xs-12 col-sm-12 col-md-8"> </select>
                                        </div>
                                    </div>
                                </div>

                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                        <div>
                                            <label style="text-align:left;">Fecha de Tramite:</label>
                                        </div>
                                        <div class="form-inline"> 
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", @placeholder="desde" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                            &nbsp; &nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", @placeholder = "hasta" })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-3">
                                        &nbsp;
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-3" style="Text-align:right;">
                                        <div>
                                            &nbsp;
                                        </div>
                                        <div>
                                            <button id="btnConsultarOAReg" class="btn btn-success"><i class="ace-icon fa fa-search"></i>Consultar</button>
                                            <button id="btnLimpiarFiltoAOReg" class="btn btn-success"><i class="ace-icon fa fa-eraser"></i>Limpiar</button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-3">
                                        &nbsp;
                                    </div>

                                </div>

                            </div>

                        </div>
                    </div><!-- fin contenido-->
                </div>
            </div>
            <!-- -->
        </div>
    </div>
        <br />
        <br />
        <br />
        
        <div class="row">
            <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                <h3>
                    Listado de organizaciones registradas.
                </h3>
            </div>
            <br />
            <div id="div-tabla" class="table-responsive margen">
                <table id="tablaOAReg" class="table table-bordered table-hover table-striped" style="width:100%">
                    <thead>
                        <tr>
                            <th rowspan="1" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle;">ID</th>
							<th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro.</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">RUC</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Razón social </th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Línea de Acción</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Ubicación</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Representante legal</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Correo eléctronico</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Teléfono</th> 
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estado</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha recepción</th>
                            <th rowspan="1" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">resultado</th>
                            <th rowspan="1" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Recepcionar</th>
                        </tr>
                        
                    </thead>
                     
                </table>
            </div>

        </div>
        </div>
    </div>


<script type="text/javascript">
    $(document).ready(function () {
       controles_RecepcionAOReg();
    });

</script>        





