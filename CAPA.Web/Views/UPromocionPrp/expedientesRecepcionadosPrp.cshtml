@{
    ViewBag.Title = "expedientesRecepcionadosPrp";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<head>
    <title> Expedientes Recepcionados PRP</title>
</head>

<!-- contenedor principal -->
<div class="main-content">

    <div class="main-content-inner">
        <!-- Inicio Ruta -->
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">

            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocion/Index">U.Promoción</a></li>
                <li class="active">Expediente Recepcionado</li>
            </ul>

        </div>
        <!-- Fin ruta -->
        <!-- contenido -->
        <div class="page-content">

            <!-- Inicio titulo -->
            <div class="page-header">
                <div class="cbp-mc-column">
                    <h1> Expedientes Recepcionados </h1>
                </div>
            </div>
            <!-- Fin titulo -->
            <!-- inicio pasos-->
            <!-- contenido de pasos -->
            <div class="col-xs-12 col-sm-12 col-md-12" style="margin-bottom:25px;">

                <!--------- ACORDEON ------------>
                @*<div id="accordion" class="col-xs-12 col-sm-12 col-md-12" style="padding:0px; border:1px solid; ">*@
                @*<div class="card col-xs-12 col-sm-12 col-md-12" style="margin:0px 0px;">*@
                <div id="accordion" style="border:1px solid;">
                    <div class="card">

                        @*<div class="card-header widget-header col-xs-12 col-sm-12 col-md-12" id="headingOne" style="text-align:left;">*@
                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-search"></i>
                                    Criterios de Búsqueda
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>

                        <!--------- card body ------------>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                            <!-- Inicio Card Body  -->
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">

                                <div class="row" style="margin-bottom:10px;">
                                    <div class="col-xs-12 col-sm-12 col-md-3 panel panel-default" style="margin-left:15px; margin-top:10px;">
                                        <div class="panel-body inline-block col-xs-12 col-sm-8 col-md-12" style="text-align:left">
                                            <label class="col-xs-12 col-sm-6 col-md-4"> Buscar Por: </label>
                                            <label class="col-xs-12 col-sm-6 col-md-4"><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                            <label class="col-xs-12 col-sm-6 col-md-4"><input id="rb_sgd" type="radio" name="optradio"> Nro. CUT</label>

                                        </div>
                                    </div>
                                </div>


                                <div id="conRuc" class="row" style="margin-bottom:10px;">
                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>RUC :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("idtipoSda", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '2' })
                                            @Html.TextBox("idproceso", (string)"", new { @class = "campoNumero",  style = "display:none; width:50%", Value = '2' }) 
                                            @Html.TextBox("idtipoIncentivo", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '0' })
                                            @Html.TextBox("idUnidadPcc", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "display:none; width:50%", Value = '2' })
                                            @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:50%" })
                                            <button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. Expediente :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("idExpedienteOA", (string)"", new { @class = "", @MaxLenght = "17", style = "display:none; width:50%", disabled = true })
                                            @Html.TextBox("nroExpedienteOA", (string)"", new { @class = "", @MaxLenght = "17", style = "width:60%", disabled = true })
                                            <p id="porprocesoPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP" style="display:none; font-size:10px; color:darkred">Este expedientecorresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP" style="display:none; font-size:10px; color:darkred">Este expediente corresponde al proceso de Planes de Negocio. </p>
                                            @*<select id="cbxExpedientesFl" class="custom-select" style="width:60%;"> </select>*@
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. CUT :</label>
                                        </div>
                                        <div>
                                            <select id="cbxCutExpedientesFl" class="custom-select" style="display:none; width:60%;"> </select>
                                            @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", disabled = true })
                                            <p id="porprocesoPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP2A" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Estado actual:</label>
                                        </div>
                                        <div>
                                            @{List<KeyValuePair<int, string>> ddl_Estado = new List<KeyValuePair<int, string>>();
                                                ddl_Estado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(1, "Sin Asignar"));
                                                ddl_Estado.Add(new KeyValuePair<int, string>(2, "Asignado a Especialista"));
                                                SelectList selectEstado = new SelectList(ddl_Estado, "key", "value", 0);
                                            }

                                            @Html.DropDownList("estadoFiltro", selectEstado, new { style = "width:60%" })
                                        </div>
                                    </div>

                                </div>

                                <div id="conSGD" class="row" style="margin-bottom:10px; display:none">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Nro. CUT :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("nroCutSgd2", (string)"", new { @class = "", style = "width:60%", })
                                            <p id="porprocesoPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion PRP. </p>
                                            <p id="porprocesoPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde a la linea de Accion Planes de Negocio. </p>
                                            <p id="porlineaAccionPN2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de PRP. </p>
                                            <p id="porlineaAccionPRP2B" style="display:none; font-size:10px; color:darkred">Este nro. Cut corresponde al proceso de Planes de Negocio. </p>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Estado :</label>
                                        </div>
                                        <div>
                                            @{List<KeyValuePair<int, string>> ddl_EstadoSGD = new List<KeyValuePair<int, string>>();
                                                ddl_EstadoSGD.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                                ddl_EstadoSGD.Add(new KeyValuePair<int, string>(1, "Sin Asignar"));
                                                ddl_EstadoSGD.Add(new KeyValuePair<int, string>(2, "Asignado a Especialista"));
                                                SelectList selectEstadoSGD = new SelectList(ddl_EstadoSGD, "key", "value", 0);
                                            }
                                            @Html.DropDownList("estadoFiltroSGD", selectEstado, new { style = "width:60%" })
                                        </div>
                                    </div>

                                </div>

                                <div class="row" style="margin-bottom:10px;">
                                    <div class="col-xs-12 col-md-6" style="text-align:left">
                                        <div>
                                            <label>Razón Social :</label>
                                        </div>
                                        <div>
                                            @Html.TextBox("razSocial", (string)"", new { @class = "campoTexto", style = "width:80%" })
                                        </div>
                                    </div>
                                </div>

                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Departamento :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDepartamentoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Provincia :</label>
                                        </div>
                                        <div>
                                            <select id="cbxProvinciaFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-3" style="text-align:left">
                                        <div>
                                            <label>Distrito :</label>
                                        </div>
                                        <div>
                                            <select id="cbxDistritoFl" class="custom-select" style="width:60%;"> </select>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                        <div>
                                            <label style="text-align:left;">Fecha de Tramite:</label>
                                            @Html.CheckBox("chkActFechaFl")
                                        </div>
                                        <div class="form-inline">
                                            desde&nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", disabled = true })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                            &nbsp;&nbsp; hasta&nbsp;
                                            <div class="input-group date" style="width:35%; Text-align:left;">
                                                @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", disabled = true })
                                                <span class="input-group-addon" style="text-align:left; width:5%;">
                                                    <span class="glyphicon glyphicon-calendar"></span>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-12 col-md-4" style="text-align:left">

                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align:center;">
                                        <div>  &nbsp; </div>
                                        <div>
                                            <button id="btnConsultarExpedientesRecepcionados" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>
                                            &nbsp; &nbsp;
                                            <button id="btnLimpiarFiltroExpedientesRecp" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 col-md-4" style="text-align:left;">
                                        <div>  &nbsp; </div>

                                    </div>

                                </div>
                                <br />


                            </div>
                            <!-- Fn Card Body  -->
                        </div>

                    </div>
                </div>
                <!-- fin de Acrodeon -->

            </div>
            <!-- fin contenido pasos-->

        </div>
        <!-- fin contenido-->

        <br />
        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">
            <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                <h3>
                    Lista de Expedientes Recepcionados
                </h3>
            </div>

            <div class="table-responsive margen">
                <table id="tab_ExpRecepcionados" class="table table-bordered table-hover table-striped display" style="width:100%">
                    <thead>
                        <tr>
                            <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro.</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">RUC</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">Razón social</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Tipo cofinanciamiento</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Proceso</th>
                            <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Tipo incentivo</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">N°. Expediente</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">N°. CUT</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Asunto</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Unidad destino</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Lugar de recepción</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha recepción</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Estado actual</th>
                            <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha asignación</th>
                            <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Reasignación</th>
                            <th colspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                        </tr>
                        <tr>
                            @*<th class="center">Ver Detalle</th>*@
                            <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Asignar Especialista</th>
                            <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Reasignar Especialista</th>
                        </tr>
                    </thead>
                </table>


            </div>



        </div>

    </div>




</div>


<script type="text/javascript">
    $(document).ready(function () {
        controles_ExpRecepcionados();

    });
</script>



