
@{
    ViewBag.Title = "padronSocios_PRP";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<head>
    <title>Padrón de Socios OA-PRP</title>
</head>

<div class="main-content">
<!-- contenedor -->

<div class="main-content-inner">
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">
        <!-- INICIO RUTA -->
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocion/Index">U.Promoción PRP</a></li>
            <li class="active"><a href="/UPromocionPrp/padronSocios_PRP">Padrón de Socios OA-PRP</a></li>
        </ul>

    </div><!-- FIN RUTA -->

    <div class="page-content">
        <!-- contenido -->

        <div class="page-header">
            <!-- Inicio titulo -->

            <div class="cbp-mc-column">
                <h1> Padrón de Socios </h1>
            </div>

        </div>
        <!-- Fin titulo -->
        <!-- inicio pasos-->
        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">
            <!-- contenido de pasos -->
            <!--------- ACORDEON ------------>

            <div id="accordion" style="border:1px solid; ">
                <div class="card">

                    <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Padrón de socios de OA-PRP
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <!--------- card body ------------>
                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                        <div class="card-body" style="padding-left:12px;padding-right:12px;">

                            <div class="row" style="margin-bottom:10px;">
                                <div class="col-xs-12 col-md-12 panel panel-default" style="width:20%; margin-left:1%; margin-top:1%;">
                                    <div class="panel-body inline-block" style="text-align:left">
                                        <label>Buscar Por:</label>
                                        &nbsp;&nbsp;&nbsp;
                                        <label><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                        &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;
                                        <label><input id="rb_sgd" type="radio" name="optradio"> Nro. CUT</label>
                                    </div>
                                </div>
                            </div>

                            <div id="conRuc" class="row" style="margin-bottom:10px; margin-left:auto">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>RUC:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })

                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>N° Expediente:</label>
                                    </div>
                                    <div>
                                        <select id="cbxExpedientes" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>N° CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", disabled = true })
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left;">
                                    <div>
                                        <label style="text-align:left;">
                                            Razón Social :
                                        </label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razon_social", (string)"", new { @MaxLength = "500", style = "width:70%", })
                                    </div>
                                </div>

                            </div>

                            <div id="conSGD" class="row" style="margin-bottom:10px; display:none; margin-left:auto">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", })
                                    </div>
                                </div>

                            </div>


                            <div class="row" style="margin-bottom:10px; margin-left:auto">

                               
                                <div class="col-xs-12 col-md-4" style="text-align:left;">
                                    <div>
                                        <label style="text-align:left;">
                                            Razón social :
                                        </label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razon_social", (string)"", new { @MaxLength = "500", style = "width:90%", disabled = "true" })
                                    </div>
                                </div>
                                
                                 <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>Nombre de PRP:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nombrePRP", (string)"", new { @class = "", style = "width:90%", disabled = true })
                                    </div>
                                </div>

                                <div class="col-xs-12  col-md-4" style="text-align:left;">
                                    <div>  &nbsp; </div>
                                    <div>
                                        <button id="btnConsultarPSocios" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>  &nbsp;&nbsp;&nbsp;
                                        <button id="btnLimpiarConsultaPSocios" class="btn btn-warning" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                    </div>
                                </div>

                                <br />

                            </div>

                            <br />
                        </div>
                    </div>

                </div>

               

            </div>
            <!-- fin de Acrodeon -->
        </div>
        <!-- fin  pasos-->
    </div>
    <!-- fin contenido-->

    <!--TABLA PADRON SOCIOS-->
    <div class="col-xs-12" style="margin-bottom:25px;">
        <!-- inicio tabla-->
        <div class="table-responsive margen">
            <br />
            <table id="TablaPadronSocioPRP" class="table table-bordered table-hover table-striped display" style="width:100%">
                <thead>
                    <tr>
                        <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">Nro.</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">RUC</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:20%; max-width:25%">Razón social</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:12%">Ubicación</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:12%">Actividad económica</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Titulada</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Sin título</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Bajo riego</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Secano</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Pastizales</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Nro. Cabeza</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Ha. Destinada al PCC</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:8%">Total socios</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:10%">Registrar padrón</th>
                        
                </thead>

                <tbody>
                    <tr>
                       
                        <td class="center">01</td>
                        <td class="center">Arequipa </td>
                        <td style="text-align:center"> 15/10/2016 </td>
                        <td class="center">20/11/2018 </td>
                        <td class="center"> - </td>
                        <td class="center"> - </td>
                        <td class="center"> - </td>
                        <td class="center">-</td>
                        <td class="center">-</td>
                        <td class="center"> - </td>
                        <td class="center"> - </td>
                        <td class="center"> - </td>
                        <td class="center">-</td>
                        <td class="center">
                            <a class="btn btn-primary" id="btnRegistraPadron" href="#" title="Registrar" data-toggle="modal">
                                <i class="ace-icon fa fa-pencil"></i>
                            </a>
                        </td>

                    </tr>
                </tbody>

            </table>

        </div>

    </div>
    <!-- FIN TABLA PADRON SOCIOS-->


</div>
<!-- fin contenido-->
</div>
<!-- fin contenedor -->



<script type="text/javascript">
    $(document).ready(function () {
        controles_UP_PRP_Socios();

    });

</script>
