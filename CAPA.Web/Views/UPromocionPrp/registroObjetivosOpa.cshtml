@{
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<head>
    <title>registró de Objetivos - Datos de la Organización</title>  
</head>

<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocionPrp/Index">U.Promoción</a></li>
                <li><a href="/UPromocionPrp/registrarByS">Registrar Objetivos</a></li>
                <li class="active">Registrar Objetivos - Datos de la Organización </li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->

            <div class="page-header">
                <!-- Inicio titulo -->

                <div class="col-xs-12" style="text-align:left">

                    <h1> Registrar Objetivos - Datos de la Organización</h1>
                </div>
            </div>       <!-- Fin titulo -->
            <!-- inicio pasos-->

            <div class="col-xs-12">
                <!-- contenido de pasos -->

                <div class="row">
                    <div class="col-xs-12" style="text-align:left; margin-bottom:1%;">
                        <h3>
                            <i class="ace-icon fa fa-file"></i>
                            Datos de la Organización
                            <hr />
                        </h3>
                    </div>

                </div>
                <div class="row" style="margin-bottom:25px;">
                    <div class="col-md-3" style="text-align:left;">

                        <label style=" text-align:left;">
                            R.U.C :
                        </label>

                        @Html.TextBox("nro_orden", (string)"20570607945", new { @class = "solo-numero", style = "width:40%", disabled = "true" })

                    </div>
                    <div class="col-md-6" style="text-align:left;">
                        <label style=" text-align:left;">
                            Razón Social :
                        </label>
                        @Html.TextBox("razon_social", (string)"ASOC. DE AGRIC. AGROPEC. 'RAYOS DEL DIAMANTE'", new { @MaxLength = "500", style = "width:70%", disabled = "true" })

                    </div>
                    <div class="col-md-3" style="text-align:left;">

                        <label style=" text-align:left;">
                            Nro de Expediente :
                        </label>

                        @Html.TextBox("nro_orden", (string)"PRP-130-2018", new { @class = "solo-numero", style = "width:40%", disabled = "true" })

                    </div>
                </div>
                <div class="row" style="margin-bottom:25px;">
                    <div class="col-md-3" style="text-align:left;">

                        <label style=" text-align:left;">
                            Fecha Inscripción en SUNARP :
                        </label>

                        @Html.TextBox("nro_orden", (string)"20/10/2014", new { @class = "solo-numero", style = "width:40%" })

                    </div>
                    <div class="col-md-3" style="text-align:left;">
                        <label style=" text-align:left;">
                            Fecha de Constitución :
                        </label>
                        @Html.TextBox("razon_social", (string)"21/10/2014", new { @MaxLength = "500", style = "width:40%" })

                    </div>
                    <div class="col-md-3" style="text-align:left;">

                        <label style=" text-align:left;">
                            FECHA REGISTRO en S.E.L :
                        </label>

                        @Html.TextBox("nro_orden", (string)"01/05/2015", new { @class = "solo-numero", style = "width:40%", disabled = "true" })

                    </div>
                    <div class="col-md-3" style="text-align:left;">

                        <label style=" text-align:left;">
                            Fecha de Expediente :
                        </label>

                        @Html.TextBox("nro_orden", (string)"07/05/2015", new { @class = "solo-numero", style = "width:40%", disabled = "true" })

                    </div>
                </div>





                <br />
                <div class="row" style="margin-bottom:25px;">
                    <br />
                    <a class="btn btn-success" id="" href="#registróObjetivo" title="Registrar Objetivos" data-toggle="modal">
                        <i class="ace-icon fa fa-pencil"></i>REGISTRAR OBEJTIVO
                    </a>
                    
                    <a class="btn btn-success" id="" href="/UPromocionPrp/registróObjetivos" title="Cancelar">
                        <i class="ace-icon fa fa-undo"></i>CANCELAR
                    </a>

                </div>
                <br />

                <div id="accordion" style="border:1px solid;">
                    <div class="card">
                        <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                            <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:white; text-decoration:none;">
                                    <i class="ace-icon fa fa-file"></i>
                                    Lista de Objetivos Registrados
                                </a>
                            </h5>
                            <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                <a href="#" data-action="collapse" style="color:#fff;">
                                    <i class="ace-icon fa fa-chevron-down"></i>
                                </a>
                            </div>
                        </div>
                        <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                            <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                <br />

                                <div class="row">
                                    <div class="col-xs-12">
                                        <!-- inicio tabla-->
                                        <div class=" table-responsive margen">
                                            <table id="tab_bien" class="table  table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th width="3%" class="center">Nº</th>
                                                        <th width="10%" class="center">Objetivo General</th>
                                                        <th width="15%" class="center">Objetivo Específico</th>
                                                        <th width="8%" class="center">Unidad de Medida</th>
                                                        <th width="7%" class="center">Cantidad Esperada</th>
                                                        <th width="15%" class="center">&nbsp;</th>
                                                    </tr>
                                                </thead>

                                                <tbody>
                                                    <tr>
                                                        <td class="center">1</td>
                                                        <td class="center">RECONVERSIÓN DE CULTIVO ACTUAL  </td>
                                                        <td style="text-align:left"> INSTALACION DE NUEVO CULTIVO </td>
                                                        <td class="center"> Ha. </td>
                                                        <td class="center"> 35</td>
                                                        <td>
                                                            <a class="btn btn-success" id="" href="" title="Modificar" data-toggle="modal">
                                                                <i class="ace-icon fa fa-edit"></i>
                                                            </a>
                                                            <a class="btn btn-danger" id="" href="" title="Eliminar" data-toggle="modal">
                                                                <i class="ace-icon fa fa-trash"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                    <tr>
                                                        <td class="center">2</td>
                                                        <td class="center">RECONVERSIÓN DE CULTIVO ACTUAL  </td>
                                                        <td style="text-align:left"> PRODUCCION DE NUEVO CULTIVO </td>
                                                        <td class="center"> TM. </td>
                                                        <td class="center"> 250</td>
                                                        <td>
                                                            <a class="btn btn-success" id="" href="" title="Modificar" data-toggle="modal">
                                                                <i class="ace-icon fa fa-edit"></i>
                                                            </a>
                                                            <a class="btn btn-danger" id="" href="" title="Eliminar" data-toggle="modal">
                                                                <i class="ace-icon fa fa-trash"></i>
                                                            </a>
                                                        </td>
                                                    </tr>
                                                </tbody>
                                            </table>
                                        </div>

                                    </div>
                                </div><br />
                            </div>
                        </div>
                    </div>

                </div>

                <div id="registróObjetivo" class="modal fade">
                    <div class="modal-dialog">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                                <h4 class="modal-title">Registrar Objetivo</h4>
                            </div>
                            <div class="modal-body">
                                
                                <p>Datos del Objetivo</p>
                                <hr />
                                <div class="row" style="margin-bottom:5px;">
                                    <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">Objetivo General :</label></div>
                                    <div class="col-xs-8">
                                        @{
                                            List<KeyValuePair<int, string>> dropdownList9 = new List<KeyValuePair<int, string>>();
                                            dropdownList9.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                            dropdownList9.Add(new KeyValuePair<int, string>(1, "RECONVERSIÓN DE CULTIVO ACTUAL"));
                                            SelectList selectList9 = new SelectList(dropdownList9, "key", "value", 0);

                                        }
                                        @Html.DropDownList("objetivoG", selectList9, new { style = "width:100%" })
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:5px;">
                                    <div class="col-xs-4" style="text-align:left;"><label style=" text-align:left;">Objetivo Específico : </label></div>
                                    <div class="col-xs-8">
                                        @{
                                            List<KeyValuePair<int, string>> dropdownList1 = new List<KeyValuePair<int, string>>();
                                            dropdownList1.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                            dropdownList1.Add(new KeyValuePair<int, string>(1, "INSTALACIÓN DE NUEVO CULTIVO"));
                                            dropdownList1.Add(new KeyValuePair<int, string>(2, "PRODUCCIÓN DE NUEVO CULTIVO"));
                                            dropdownList1.Add(new KeyValuePair<int, string>(3, "PRODUCTIVIDAD DE NUEVO CULTIVO"));
                                            SelectList selectList1 = new SelectList(dropdownList1, "key", "value", 0);

                                        }
                                        @Html.DropDownList("objetivoE", selectList1, new { style = "width:100%" })
                                    </div>
                                </div>
                                <div class="row" style="margin-bottom:5px;">
                                    <div class="col-xs-4" style="text-align:left;"><label style="text-align:left;">Cantidad Esperada :</label></div>
                                    <div class="col-xs-8" style="text-align:left;">@Html.TextBox("nro_orden", (string)"", new { style = "width:30%" }) <label id="unidadMedida" style="text-align:left;"></label></div>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-success" data-dismiss="modal">Agregar Objetivo</button>
                                <button type="button" class="btn btn-danger" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <br />
                <br />
      
                <!-- -->
            </div>
            <div class="row">
                <!-- inicio grid-->
                <!-- Fin tabla-->


            </div><!-- fin grid -->
        </div><!-- contenido -->
    </div>

</div><!-- fin contenedor -->
<script>
    $(document).ready(function () {
        var select = document.getElementById('objetivoE');
        select.addEventListener('change',
          function () {
              var selectedOption = this.options[select.selectedIndex];
              if (selectedOption.value == 0) {
                  document.getElementById('unidadMedida').innerHTML = '';
              }
              if(selectedOption.value == 1){
                  document.getElementById('unidadMedida').innerHTML = 'Has.';
              }
              if (selectedOption.value == 2) {
                  document.getElementById('unidadMedida').innerHTML = 'TM.';
              }
              if (selectedOption.value == 3) {
                  document.getElementById('unidadMedida').innerHTML = 'TM.';
              }
          });
    });
    

    $("#tab_bien").DataTable({
        "ordering": false,
        "lengthChange": false,
        "searching": false,
        //"info": false,
        "language": {
            "paginate": {
                "previous": "Anterior",
                "next": "Siguiente"
            }
        }
    });

    $("#tab_servicio").DataTable({
        "ordering": false,
        "lengthChange": false,
        "searching": false,
        //"info": false,
        "language": {
            "paginate": {
                "previous": "Anterior",
                "next": "Siguiente"
            }
        }
    });

</script>



