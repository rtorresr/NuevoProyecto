
@{
    ViewBag.Title = "evaluacionDoc_FichaExp_SocioPRP";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<title>Evaluación documentaria: Ficha inventario expediente(PRP-Socio)</title>

<div class="main-content">
<!-- contenedor -->

<div class="main-content-inner">
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">
        <!-- Fin Ruta -->
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocionPrp/Index">U.Promoción PRP</a></li>
            <li class="active">Ficha Inventario Expediente(PRP-Socio)</li>
        </ul>

    </div><!-- Inicio ruta -->

    <div class="page-content">
        <!-- contenido -->

        <div class="page-header">
            <!-- Inicio titulo -->

            <div class="cbp-mc-column">

                <h1> Evaluación documentaria: Ficha inventario expediente(PRP-Socio) </h1>
            </div>
        </div>       <!-- Fin titulo -->
        <!-- inicio pasos-->


        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">


            <!--------- ACORDEON ------------>

            <div id="accordion" style="border:1px solid; ">
                <div class="card">

                    <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Evaluación documentaria PRP - Socios
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <!--------- card body ------------>
                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                        <!-- Inicio Card Body  -->
                        <div class="card-body" style="padding-left:12px;padding-right:12px;">

                            <br />
                            <div class="row" style="margin-bottom:15px; margin-left:auto">

                                <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>DNI:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("dniSocio", (string)"", new { @class = "campoNumero", @MaxLenght = "11", disabled = true, style = "width:70%" })
                                        @*<button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>*@
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>Socio:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("socio", (string)"", new { @Class = "camposTexto", disabled = true, style = "width:70%", })
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>Razón social:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razonSocial", (string)"", new { @Class = "camposTexto", disabled = true, style = "width:70%", })
                                    </div>
                                </div>

                            </div>


                            <div class="row" style="margin-bottom:15px; margin-left:auto">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. Expediente:</label>
                                    </div>
                                    <div>
                                        <select id="cbxNroExpediente" class="custom-select" style="width:70%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:70%", disabled = true })
                                    </div>
                                </div>
                                
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. Ha a reconvertir:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("especialista", (string)"", new { @class = "", style = "width:70%", disabled = true })
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Oficina regional</label>
                                    </div>
                                    <div>
                                        <select id="cbxOficinaRegionalFl" class="custom-select" style="width:70%;"> </select>
                                    </div>
                                </div>

                            </div>

                            <div class="row" style="margin-bottom:15px; margin-left:auto">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Motivo de actualización:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("motivoAct", (string)"", new { @class = "", style = "width:70%" })
                                    </div>
                                </div>
                                
                                 <div class="col-xs-12 col-md-3" style="text-align:left;">
                                    <div>  &nbsp; </div>
                                    <div>
                                        <button id="btnMostrarEvaluacionDoc" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i> Guardar</button>
                                        &nbsp; &nbsp;
                                        <button id="btnModificaEvaluacionDoc" class="btn btn-warning" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i> Modificar </button>
                                        &nbsp; &nbsp;
                                        <button id="btnCerrar" class="btn btn-danger" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i> Cerrar </button>
                                    </div>
                                </div>

                            </div>

                            <br />
                        </div>
                        <!-- Fn Card Body  -->
                    </div>

                    <!-- INICIO 2DO ACORDEON -->

                    <div class="card-header widget-header" id="fichaInventarioPRP" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapsetwo" aria-expanded="true" aria-controls="collapsetwo">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Registrar evaluación documentaria
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapsetwo" aria-expanded="true" aria-controls="collapsetwo">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <div id="collapsetwo" class="collapse" aria-labelledby="fichaInventarioPRP" data-parent="#accordion">

                        <!-- Inicio Card Body  -->
                        <div class="card-body" style="padding-left:12px;padding-right:12px;">
                            <br />
                            <!-- TABLA CHECKLIST FRMTO O-1 -->
                            <h3>Registrar documentos presentados por el socio</h3>
                            <table id="tablaFichaExpSocioPRP" class="table  table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>

                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:20%">Seleccionar el documento</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:10%">Cumple documento </th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Tipo Doc. propiedad</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Fecha de documento</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Nro. hectáreas a reconvertir</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Cantidad agua asignada</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Nro. Resolución Directoral</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:25%; max-width:30%">Observación</th>
                                    <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:10%">Acción</th>
                                    @*<th colspan="3" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Acciones</th>
                </tr>
                <tr>
                    <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Modificar</th>
                    <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Eliminar</th>
                </tr>*@
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="center">
                                        <select id="cbxDocumentotb" class="custom-select" style="width:100%;"> </select>
                                    </td>
                                    <td class="center">
                                        <select id="cbxCumpleDoctb" class="custom-select" style="width:70%;"> </select>
                                    </td>
                                    <td class="center">
                                        <select id="cbxTipoDoctb" class="custom-select" style="width:70%;"> </select>
                                    </td>
                                    <td class="center">
                                        <div class="col-xs-12" style="text-align:left;">@Html.TextBox("txbfechaDocumento", (string)"", new { style = "width:100%", })</div>
                                    </td>
                                    <td class="center">
                                        <div class="col-xs-12" style="text-align:left;">@Html.TextBox("txbNroHectareas", (string)"", new { style = "width:100%", })</div>
                                    </td>
                                    <td style="text-align:center">
                                        <div class="col-xs-12" style="text-align:left;">@Html.TextBox("txbcantidadAgua", (string)"", new { style = "width:100%", })</div>
                                    </td>
                                    <td class="center">
                                         <div class="col-xs-12" style="text-align:left;">@Html.TextBox("txbResDirectoral", (string)"", new { style = "width:100%", })</div>
                                    </td>
                                    <td class="center">
                                        <div class="col-xs-12" style="text-align:left;">@Html.TextBox("txbObservacion", (string)"", new { style = "width:100%", })</div>
                                    </td>
                                    <td class="center">
                                        <button class="btn btn-success" id="btnAgregarDocSocioPRP" href="" title="Agregar Documentos Socio PRP">
                                            <i class="ace-icon fa fa-plus"></i>Agregar
                                        </button>
                                    </td>
                                    </tr>
                            </tbody>

                            </table>


                        </div>
                    </div>
                    <!--  FIN 2DO ACORDEON-->

                </div>
            </div>
            <!-- fin de Acrodeon -->

        </div>

        <br>
        <br>

        <!-- TABLA DOC QUE FORMARAN PARTE PRPA -->
        <div class="table-responsive margen">
            <h3 style="text-align:left;">Lista de documentos registrados del socio</h3>
            <table id="tabResultadosEvaluacionDoc" class="table table-bordered table-hover table-striped display" style="width:100%; margin-left:auto">
                <thead>
                    <tr>
                        <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:20%">Nro.</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:20%">Documento</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:6%; max-width:10%">Cumple</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Tipo Doc. propiedad</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Fecha de documento</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Nro. hectáreas a reconvertir</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Cantidad de agua asignada</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Nro. Resolución Directoral</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:25%; max-width:30%">Observación</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:10%">Modificar</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:10%">Eliminar</th>
                        @*<th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:15%; max-width:25%">Eliminar</th>*@
                    </tr>
                </thead>

                @*<tbody>
                    <tr>
                        @*<td class="center">1</td>*@
                        <td class="center">01 </td>
                        <td class="center">Todos </td>
                        <td style="text-align:center">Si </td>
                        <td class="center">-- </td>
                        <td class="center">
                            <button id="btnVerDocAdjunto" class="btn btn-warning" style="margin-bottom: 5px; vertical-align: middle; text-align: center;" title="Ver Doc. Adjunto"><i class="ace-icon fa fa-eye"></i> </button>
                        </td>

                </tbody>

            </table>

            <div class="row" style="margin-bottom:20px;">

                <div class="col-xs-12 col-md-3" style="text-align:left">
                    <div>
                        <label style="text-align: left;">Resultado final:</label>
                    </div>
                    <div>
                        @Html.TextBox("resultFinalFrmto_I1", (string)"", new { @class = "", style = "width:70%", disabled = true })
                    </div>
                </div>

                <div class="col-xs-12 col-md-3" style="text-align:left">
                    <div>
                        <label style="text-align: left;">Recomendación:</label>
                    </div>
                    <div>
                        @Html.TextBox("resultFinalFrmto_I1", (string)"", new { @class = "", style = "width:70%", })
                    </div>
                </div>

            </div>

        </div>
        <br />
        <br />



        <div class="row">
            <!-- inicio grid-->
            <!-- Fin tabla-->


        </div><!-- fin grid -->
    </div><!-- contenido -->
</div>

</div><!-- fin contenedor -->
