
@{
    ViewBag.Title = "cargaLaboralEspecialistaPRP";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<title>Carga laboral especialista PRP</title>


<div class="main-content">

<div class="main-content-inner">
    <!-- Inicio Ruta -->
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">

        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocionPrp/Index">U.PromociónPRP</a></li>
            <li class="active">Carga laboral especialista PRP</li>
        </ul>

    </div>
    <!-- Fin ruta -->
    <!-- contenido -->
    <div class="page-content">

        <!-- Inicio titulo -->
        <div class="page-header">
            <div class="cbp-mc-column">
                <h1> Carga laboral especialista PRP</h1>
            </div>
        </div>
        <!-- Fin titulo -->
        <!-- inicio pasos-->
        <!-- contenido de pasos -->
        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">


            <!--------- ACORDEON ------------>

            <div id="accordion" style="border:1px solid; ">
                <div class="card">

                    <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Criterios de Búsqueda
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <!--------- card body ------------>
                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                        <!-- Inicio Card Body  -->
                        <div class="card-body" style="padding-left:12px;padding-right:12px;">
                            <div id="cargaTrabajo" class="row" style="margin-bottom:10px; margin-top:30px; margin-left:auto">

                                <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>Especialista:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("txbEspecialista", (string)"", new { @class = "", style = "width:65%", disabled = true })
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align:left">
                                    <div>
                                        <label>Oficina regional:</label>
                                    </div>
                                    <div>
                                        <select id="cbxOficinaRegionalFl" class="custom-select" style="width:65%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-4" style="text-align: left; margin-bottom: 5px;">
                                    <div class="inline-block">
                                        <label style="text-align:left;">Fecha de Tramite:</label>
                                        @Html.CheckBox("chkActFechaFl")
                                    </div>
                                    <div class="form-inline">
                                        desde&nbsp;
                                        <div class="input-group date" style="width:35%; Text-align:left;">
                                            @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", disabled = true })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                        &nbsp;&nbsp; hasta&nbsp;
                                        <div class="input-group date" style="width:35%; Text-align:left;">
                                            @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;", disabled = true })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>


                            <div class="row" style="margin-bottom:10px;">

                                <div class="col-xs-12 col-md-12" style="text-align:center;">
                                    <div>  &nbsp; </div>
                                    <div>
                                        <button id="btnConsultarCargaTrabajo" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>
                                        &nbsp;&nbsp;
                                        <button id="btnLimpiarCargaTrabajo" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                    </div>
                                </div>
                            </div>
                            <br />
                        </div>
                        <!-- Fn Card Body  -->
                    </div>
                </div>
            </div>
            <!-- fin de Acrodeon -->
        </div>
    </div>
    <!-- fin contenido-->

    <br />
    <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">

        <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
            <h3>
                Carga laboral de los especialistas
            </h3>
        </div>
        <div class="table-responsive margen">
            <table id="tablaExpedienteAsig" class="table table-bordered table-hover table-striped display" style="width:100%">
                <thead>
                    <tr>
                        <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro.</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Apellidos y Nombres</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:8%">Oficina Regional</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:8%; max-width:8%">Total de Expedientes Asignados</th>

                        <th colspan="6" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Expedientes de Planes de Negocio</th>

                        <th colspan="7" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Expedientes de PRP</th>

                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Asignar Especialista</th>
                    </tr>
                    <tr>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En evaluación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En Observación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En Re-Evaluación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Elegible</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Improcedente</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Otros Planes de Negocio</th>

                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En evaluación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En Observación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En Re-Evaluación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Opinión Técnica Favorable</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">En formulación de Proyecto</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Informe de Formulación</th>
                        <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Otros PRP</th>
                    </tr>


                    @*<tr>
                            <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:3%">Asignar Especialista</th>
                        </tr>*@
                </thead>
            </table>

        </div>
    </div>



</div>

</div>

<script>
    $(document).ready(function () {
        controles_AsignacionExpedientes();

    })
</script>
