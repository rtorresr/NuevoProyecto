
@{
    ViewBag.Title = "periodoEstado";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<head>
    <title>Plazos de Estados</title>
</head>

<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <span>Bienvenido  Sr: @Session["NombreUsua"] </span>

            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UPromocionPrp/Index">U.PromocionPrp</a></li>
                <li class="active">Plazo del Estado</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->
            <!-- Inicio titulo -->
            <div class="page-header">
                <div class="cbp-mc-column">
                    <h1>Plazo del Estado</h1>
                </div>
            </div>
            <!-- Fin titulo -->


            <div class="col-md-12">
                <div class="nav-tabs-custom">

                    <div class="tab-content">

                        <div class="active tab-pane" id="pesProrroga">

                            <div class="row">
                                <div class="col-xs-12 col-md-11" style="text-align: left; margin-left: 1%; border-bottom: 1px solid #ede9e9;">
                                    <h3>
                                        <i class="ace-icon fa fa-file"></i>
                                        DATOS DE PRÓRROGA
                                    </h3>
                                </div>
                            </div>

                            <br />
                            <div>
                                <div class="row" style="margin-bottom:10px;">

                                    <div class="col-xs-1 col-sm-1 col-md-1">
                                        <div>&nbsp;</div>
                                    </div>


                                    <div class="col-xs-10 col-sm-10 col-md-10 panel panel-default">

                                        <div class="row" style="margin-bottom:10px">
                                            <br />

                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label> Linea de Acción:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxTipoSDAFr" class="custom-select col-xs-12 col-sm-6 col-md-8" style="margin-bottom:5px;"> </select>
                                                </div>
                                            </div>


                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label>Unidad PCC:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxUnidadProgFr" class="custom-select col-xs-12 col-sm-6 col-md-8" style="margin-bottom:5px;"> </select>
                                                </div>
                                            </div>

                                            <div id="proceso" class="col-xs-12 col-sm-12 col-md-4" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label>Proceso:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxProcesoFr" class="custom-select col-xs-12 col-sm-6 col-md-8" style="margin-bottom:5px;"> </select>
                                                </div>
                                            </div>


                                            <div id="tipoincentivo" class="col-xs-12 col-sm-12 col-md-4" style="display:none; text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label>Tipo Incentivo:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxTipoIncentivoFr" class="custom-select col-xs-12 col-sm-6 col-md-8" style="margin-bottom:5px;"> </select>

                                                </div>
                                            </div>
                                        </div>

                                        <div class="row" style="margin-bottom:10px">

                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align:left; margin-bottom:5px;">
                                                <div>
                                                    <label>Estado:</label>
                                                </div>
                                                <div>
                                                    <select id="cbxEstadosFr" class="custom-select col-xs-12 col-sm-6 col-md-8" style="margin-bottom:5px;"> </select>

                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left;">
                                                <div>
                                                    <label>Plazo:</label>
                                                </div>
                                                <div>
                                                    @Html.TextBox("idUsuar", (string)" ", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-8", style = "display:none", Value = Session["IdUsua"] })
                                                    @Html.TextBox("perfilUsuar", (string)" ", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-8", style = "display:none", Value = Session["PerfilUsua"] })
                                                    @Html.TextBox("idPeriodo", (string)" ", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-8", style = "display:none" })
                                                    @Html.TextBox("plazoEstado", (string)" ", new { @class = "campoNumero col-xs-12 col-sm-6 col-md-8" })
                                                    <p id="APlazoEstado" class="alerta" style="display:none; text-align:right; font-family:'Gill Sans'; color:darkred">Ingresar solo números en este campo.</p>
                                                </div>
                                            </div>

                                            <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left;">
                                                <div>
                                                    &nbsp;&nbsp;&nbsp;&nbsp;
                                                </div>
                                            </div>

                                        </div>

                                        <br />
                                        <div class="roW" style="text-align: center; margin-bottom: 5px;">
                                            <div class="col-xs-12 col-sm-6 col-md-2">
                                                &nbsp;&nbsp;&nbsp;&nbsp;
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-8">
                                                <button id="btnConsultarPeriodo" class="btn btn-success" style="display:none; margin-bottom:25px;">
                                                    <i class="ace-icon fa fa-search"></i> CONSULTAR
                                                </button>

                                                <button id="btnLimpiarPeriodo" class="btn btn-info" style="display:none; margin-bottom:25px;">
                                                    <i class="ace-icon fa fa-eraser"></i> LIMPIAR
                                                </button>


                                                <button id="btnGrabarPeriodo" class="btn btn-warning" style="display:none; margin-bottom:25px;">
                                                    <i class="ace-icon fa fa-floppy-o"></i> GUARDAR
                                                </button>

                                                <button id="btnCancelarPeriodo" class="btn btn-danger" style="display:none; margin-bottom:25px;">
                                                    <i class="ace-icon fa fa-undo"></i> CANCELAR
                                                </button>
                                            </div>
                                            <div class="col-xs-12 col-sm-6 col-md-2">
                                                &nbsp;&nbsp;&nbsp;&nbsp;
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-xs-12 col-sm-6 col-md-4">
                                            <div>&nbsp;&nbsp;</div>
                                        </div>

                                        <div class="col-xs-12 col-sm-6 col-md-4" style="text-align:center">
                                            <button id="btnNuevoPeriodo" class="btn btn-info" style="margin-bottom:auto;">
                                                <i class="ace-icon fa fa-pencil-square"></i> AGREGAR
                                            </button>
                                        </div>

                                        <div class="col-xs-12 col-sm-6 col-md-4">
                                            <div>&nbsp;&nbsp;</div>
                                        </div>
                                    </div>

                                    @*--- TABLA PLAZOS --*@

                                    <div class="col-xs-12 col-md-12" style="align-content:center;">
                                        <br />

                                        @*<h3 style="text-align:center; margin-bottom:15px;"><b>MANTENIMIENTO DE TABLA PERIODO</b></h3>*@
                                        <!-- inicio tabla-->

                                        <div class="row">
                                            <div class="col-xs-1  col-sm-1  col-md-1">
                                                <div> &nbsp;&nbsp; </div>
                                            </div>

                                            <div class="col-xs-10 col-sm-10 col-md-10">
                                                <div class="col-xs-12 col-sm-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                                    <h3>
                                                        Listado de plazos por Estado
                                                    </h3>
                                                </div>

                                                <div class="table-responsive margen col-xs-12 col-sm-12 col-md-12" style="align-content:center;">
                                                    <table id="tablaPlazoEstado" class="table table-bordered table-hover" style="width:100%; align-content:center;">
                                                        <thead>
                                                            <tr>
                                                                <th rowspan="2" width="2%" style="display:none; background-color:#005e3a; color:white;" class="center">ID</th>
                                                                <th rowspan="2" width="2%" style="background-color:#005e3a; color:white;" class="center">N°</th>
                                                                <th rowspan="2" width="13%" style="background-color:#005e3a; color:white;" class="center">Proceso</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Tipo de incentivo</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Unidad PCC</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Estado</th>
                                                                <th rowspan="2" width="5%" style="background-color:#005e3a; color:white;" class="center">Plazo en días</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Usuario Registro</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Fecha Registro</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Usuario ModificaciÓn</th>
                                                                <th rowspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Fecha Modificación</th>
                                                                <th colspan="2" width="10%" style="background-color:#005e3a; color:white;" class="center">Acciones</th>
                                                            </tr>
                                                            <tr>
                                                                <th class="text-center" style="background-color:#005e3a; color:white; min-width:5%; max-width:5%">Modificar</th>
                                                                <th class="text-center" style="background-color:#005e3a; color:white; min-width:5%; max-width:5%">Eliminar</th>
                                                            </tr>
                                                        </thead>

                                                    </table>
                                                </div>

                                            </div>

                                            <div class="col-xs-1 col-sm-1 col-md-1">
                                                <div> &nbsp;&nbsp; </div>
                                            </div>

                                        </div>

                                    </div>


                                    <br />


                                    <div class="col-xs-1 col-sm-1 col-md-1">
                                        <div>&nbsp;</div>
                                    </div>


                                </div>

                            </div>
                        </div>

                    </div>
                </div>

            </div>
            <!-- contenido -->
        </div>


    </div>
</div>



<script type="text/javascript">
    $(document).ready(function () {

        controles_periodo();

    });
</script>