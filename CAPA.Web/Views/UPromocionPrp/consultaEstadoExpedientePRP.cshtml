
@{
    ViewBag.Title = "consultaEstadoExpedientePRP";
    Layout = "~/Views/Shared/_LayoutMenuUpPrp.cshtml";
}

<title>Consultar estado expediente PRP</title>

<div class="main-content">

<div class="main-content-inner">
    <!-- Inicio Ruta -->
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">

        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UPromocion/Index">U.Promoción</a></li>
            <li class="active">Consultar estado del expediente PRP</li>
        </ul>

    </div>
    <!-- Fin ruta -->
    <!-- contenido -->
    <div class="page-content">

        <!-- Inicio titulo -->
        <div class="page-header">
            <div class="cbp-mc-column">
                <h1>Consultar estado del expediente PRP</h1>
            </div>
        </div>
        <!-- Fin titulo -->
        <!-- inicio pasos-->
        <!-- contenido de pasos -->
        <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">


            <!--------- ACORDEON ------------>

            <div id="accordion" style="border:1px solid;">
                <div class="card">

                    <div class="card-header widget-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-search"></i>
                                Criterios de búsqueda
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <!--------- card body ------------>
                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                        <!-- Inicio Card Body  -->
                        <div class="card-body" style="padding-left:12px;padding-right:12px;">

                            <div class="row" style="margin-bottom:10px;">

                                <div class="col-xs-12 col-md-12 panel panel-default" style="width:20%; margin-left:1%; margin-top:1%;">
                                    <div class="panel-body inline-block" style="text-align:left">
                                        <label>Buscar Por:</label>
                                        &nbsp;&nbsp;&nbsp;
                                        <label><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                        &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;
                                        <label><input id="rb_sgd" type="radio" name="optradio"> Nro. CUT</label>
                                    </div>
                                </div>

                            </div>

                            <div id="conRuc" class="row" style="margin-bottom:10px; margin-left:auto;">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>RUC:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("rucOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })
                                        <button id="btnCargarDatos" class="btn btn-info2 btn-sm" title="Cargar Datos"> <ion-icon name="search"></ion-icon></button>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. Expediente :</label>
                                    </div>
                                    <div>
                                        <select id="cbxExpedientes" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        <select id="cbxNroSGD" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>


                            </div>

                            <div id="conSGD" class="row" style="margin-bottom:10px; display:none; margin-left:auto">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroCutSgd", (string)"", new { @class = "", style = "width:60%", })
                                    </div>
                                </div>


                            </div>


                            <div class="row" style="margin-bottom:10px; margin-top:20px; margin-left:auto;">

                                <div class="col-xs-12 col-md-6" style="text-align:left">
                                    <div>
                                        <label>Razón social :</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razonSocialOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:80%" })
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroSGDCUT", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })
                                    </div>
                                </div>

                            </div>

                            <div class="row" style="margin-bottom:10px; margin-top:20px; margin-left:auto;">

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Proceso:</label>
                                    </div>
                                    <div>
                                        <select id="cbxProcesoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Estado actual del expediente:</label>
                                    </div>
                                    <div>
                                        @{List<KeyValuePair<int, string>> ddl_Estado = new List<KeyValuePair<int, string>>();
                                            ddl_Estado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                            ddl_Estado.Add(new KeyValuePair<int, string>(1, "Sin Asignar"));
                                            ddl_Estado.Add(new KeyValuePair<int, string>(2, "Asignado a Especialista"));
                                            SelectList selectEstado = new SelectList(ddl_Estado, "key", "value", 0);
                                        }

                                        @Html.DropDownList("estadoFiltro", selectEstado, new { style = "width:60%" })
                                    </div>
                                </div>


                            </div>


                            <div class="row" style="margin-bottom:10px; margin-top:20px; margin-left:auto;">


                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Departamento:</label>
                                    </div>
                                    <div>
                                        <select id="cbxDptoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Provincia:</label>
                                    </div>
                                    <div>
                                        <select id="cbxProvinciaFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Distrito:</label>
                                    </div>
                                    <div>
                                        <select id="cbxDistritoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                    <div>
                                        <label style="text-align:left;">Fecha de trámite:</label>
                                    </div>
                                    <div class="form-inline">
                                        desde&nbsp;
                                        <div class="input-group date" style="width:35%; Text-align:left;">
                                            @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                        &nbsp;&nbsp; hasta&nbsp;
                                        <div class="input-group date" style="width:35%; Text-align:left;">
                                            @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:100%; text-align:left;" })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                            </div>



                            <div class="row" style="margin-bottom:10px;">

                                <div class="col-xs-12 col-md-12" style="text-align:center;">
                                    <div>  &nbsp; </div>
                                    <div>
                                        <button id="btnConsultarEstadoExpedUP" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-search"></i>  Consultar  </button>
                                        &nbsp;&nbsp;
                                        <button id="btnLimpiarConsultarEstadoExpedUP" class="btn btn-success" style="margin-bottom: 5px; vertical-align: middle; text-align: center;"><i class="ace-icon fa fa-eraser"></i>  Limpiar  </button>
                                    </div>
                                </div>

                            </div>
                            <br />


                        </div>
                        <!-- Fn Card Body  -->
                    </div>

                </div>
            </div>
            <!-- fin de Acrodeon -->

        </div>
        <!-- fin contenido pasos-->

    </div>
    <!-- fin contenido-->

    <br />
    <div class="col-xs-12 col-md-12" style="margin-bottom:25px;">
        <div class="col-xs-12 col-md-12" style="text-align:left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
            <h3>
                Lista de expedientes recepcionados
            </h3>
        </div>

        <div class="table-responsive margen">
            <table id="tab_ConsultaExpediente" class="table table-bordered table-hover table-striped display" style="width:100%">
                <thead>
                    <tr>
                        <th rowspan="2" style="display:none; background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:2%">ID</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro.</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">RUC</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:30%; max-width:30%">Razón social</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Nro Expediente</th>

                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">N° CUT</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Proceso</th>
                        
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Unidad PCC</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Oficina regional</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:10%">Fecha estado</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estado actual</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Historial estado</th>
                        <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Ver plazo</th>
                     

                    </tr>
                </thead>

                <tbody>
                    <tr>
                        <td class="center"> 01 </td>
                        <td class="center"> 20506759120 </td>
                        <td class="center"> MINAGRI </td>
                       
                        <td class="center"> ---- </td>
                        <td class="center"> ---- </td>
                        <td class="center"> ---- </td>
                        <td class="center"> ---- </td>
                        <td class="center"> Arequipa </td>
                        <td class="center"> --- </td>

                        <td class="center"> Aprobado </td>

                        <td class="center">
                            <a class="btn btn-success" id="btnVerHistorialEstados" href="#eliminarModal" title="Historial Estado" data-toggle="modal">
                                <i class="ace-icon fa fa-file-text-o"></i>
                            </a>
                        </td>

                        <td class="center">
                            <a class="btn btn-info" id="btnVerPlazo" href="#eliminarModal" title="Ver Plazo" data-toggle="modal">
                                <i class="ace-icon fa fa-eye"></i>
                            </a>
                        </td>

                    </tr>

                </tbody>



            </table>


        </div>


    </div>

</div>


</div>



<script type="text/javascript">
    $(document).ready(function () {

        controles_AsignacionExpedientes();
        controles_ExpRecepcionados();

    });
</script>

