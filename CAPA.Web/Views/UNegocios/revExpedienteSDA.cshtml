
@{
    ViewBag.Title = "revExpedienteSDA";
    Layout = "~/Views/Shared/_LayoutMenuUn.cshtml";
}
<head>
    <title>Revisión del Expediente SDA</title>
</head>

<div class="main-content">
<!-- contenedor -->

<div class="main-content-inner">
    <div class="breadcrumbs ace-save-state" id="breadcrumbs">
        <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
        <!-- Fin Ruta -->
        <ul class="breadcrumb">
            <li>
                <i class="ace-icon fa fa-home home-icon"></i>
                <a href="/Modulos/">Modulos</a>
            </li>
            <li><a href="/UNegocios/index"> U.Negocios</a></li>
            <li class="active">Registrar Evaluación de SDA/PRPA</li>
        </ul>

    </div><!-- Inicio ruta -->

    <div class="page-content">
        <!-- contenido -->

        <div class="page-header">
            <!-- Inicio titulo -->

            <div class="cbp-mc-column">

                <h1> Registrar Evaluación de SDA/PRPA </h1>
            </div>
        </div>       <!-- Fin titulo -->
        <!-- inicio pasos-->

        <div class="col-xs-12">
            <!-- contenido de pasos -->
            <div id="accordion" style="border:1px solid;">
                <div class="card">

                    <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" style="color:white; text-decoration:none;">
                                <i class="ace-icon fa fa-file"></i>
                                &nbsp; Registrar Evaluación de SDA/PRPA
                            </a>
                        </h5>
                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                            <a href="#" data-action="collapse" style="color:#fff;">
                                <i class="ace-icon fa fa-chevron-down"></i>
                            </a>
                        </div>
                    </div>

                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                        <div class="card-body" style="padding-left:25px;padding-right:12px;">
                            <br />

                            <div class="row" style="margin-bottom:0px;">

                                <div class="col-xs-12 col-md-12 panel panel-default" style="width:20%; margin-left:1%;">
                                    <div class="panel-body inline-block" style="text-align:left">
                                        <label>Buscar Por:</label>
                                        &nbsp;&nbsp;&nbsp;
                                        <label><input id="rb_ruc" type="radio" name="optradio" checked> RUC</label>
                                        &nbsp;&nbsp; &nbsp;&nbsp; &nbsp;&nbsp;
                                        <label><input id="rb_sgd" type="radio" name="optradio"> N° CUT</label>
                                    </div>
                                </div>

                            </div>

                            <div class="row" id="conRuc"style="margin-bottom:20px;">

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div>
                                        <label style="text-align:left;">RUC:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroRucFl", (string)"", new { style = "width:50%" })

                                        <button type="button" class="btn btn-info" id="btnBuscarRUC" title="Validar"> <i class="ace-icon fa fa-search"></i></button><br />
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div> <label style="text-align:left;">Nro Expediente: </label> </div>
                                    <div>  <select id="cbxNoExpFl" class="custom-select" style="width:50%;"> </select> </div>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div>  <label style="text-align:left;">N° CUT:</label> </div>
                                    <div>
                                        @Html.TextBox("NroSgdCutFl", (string)"", new { @class = "campoNumero", style = "width:50%", disabled=true })

                                        <p id="ANroDocumento" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                    </div>
                                </div>

                              
                            </div>

                            <div class="row" id="conSGD" style="margin-bottom:20px;">

                               
                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Nro. CUT:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("nroSGD", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:60%" })
                                    </div>
                                </div>
                              

                               
                            </div>


                            <div class="row" style="margin-bottom:20px;">

                                <div class="col-xs-12 col-md-6" style="text-align:left">
                                    <div>
                                        <label>Razón social:</label>
                                    </div>
                                    <div>
                                        @Html.TextBox("razonSocialOA", (string)"", new { @class = "campoNumero", @MaxLenght = "11", style = "width:80%" })
                                    </div>
                                </div>
                                
                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    
                                    &nbsp;
                                </div>


                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div> <label style="text-align:left;">Línea de acción:</label> </div>
                                    <div>
                                        <select id="cbxTipoSdaFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>
                                
                              
                            </div>

                            <div class="row" style="margin-bottom:20px;">

                                
                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div> <label style="text-align:left;">Departamento: </label> </div>
                                    <div>  <select id="cbxRegionFl" class="custom-select" style="width:50%;"> </select> </div>
                                </div>
                                
                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div> <label style="text-align:left;">Provincia :</label> </div>
                                    <div>
                                        <select id="cbxProvinciaFl" class="custom-select" style="width:50%;"> </select>
                                    </div>
                                </div>
                                
                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div><label style="text-align:left;">Distrito :</label></div>
                                    <div>
                                        <select id="cbxDistritoFl" class="custom-select" style="width:50%;"> </select>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align:left">
                                    <div>
                                        <label>Tipo de incentivo:</label>
                                    </div>
                                    <div>
                                        <select id="cbxTipoIncentivoFl" class="custom-select" style="width:60%;"> </select>
                                    </div>
                                </div>


                            </div>

                            <div class="row" style="margin-bottom:0px;">

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                    <div>
                                        <label style="text-align:left;"><strong>Fecha de Trámite:</strong></label>
                                    </div>
                                    <div class="form-inline">
                                        Del:&nbsp;
                                        <div class="input-group date" style="width:40%; Text-align:left;">
                                            @Html.TextBox("fecIni1", (string)"", new { @class = "datetimepicker", style = "width:95%; text-align:left;" })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                        &nbsp;&nbsp; al:&nbsp;
                                        <div class="input-group date" style="width:40%; Text-align:left;">
                                            @Html.TextBox("fecIni2", (string)"", new { @class = "datetimepicker", style = "width:95%; text-align:left;" })
                                            <span class="input-group-addon" style="text-align:left; width:5%;">
                                                <span class="glyphicon glyphicon-calendar"></span>
                                            </span>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    &nbsp; &nbsp;
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    &nbsp; &nbsp;
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div> <label style="text-align:left;">Estado actual: </label> </div>
                                    <div>
                                        @{
                                            List<KeyValuePair<int, string>> ddlstEstado = new List<KeyValuePair<int, string>>();
                                            ddlstEstado.Add(new KeyValuePair<int, string>(0, "Seleccione..."));
                                            ddlstEstado.Add(new KeyValuePair<int, string>(1, "NUEVO"));
                                            ddlstEstado.Add(new KeyValuePair<int, string>(2, "RECEPCIONADO"));
                                            SelectList selectEstado = new SelectList(ddlstEstado, "key", "value", 0);
                                        }
                                        @Html.DropDownList("cbxEstadoFl", selectEstado, new { style = "width:60%" })
                                    </div>

                                </div>

                            </div>


                                <div class="row">
                                    <div class="cbp-mc-column">
                                    </div>

                                    <div class="cbp-mc-column">
                                    </div>

                                    <div class="cbp-mc-column">
                                    </div>

                                    <!-- columna  3 -->

                                    <br />
                                    <br />
                                    <div class="cbp-mc-column ">
                                        <button id="btnConsultarExp" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-search"></i>Buscar</button>
                                        &nbsp;
                                        <button id="btnLimpiarFiltro" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-eraser"></i>Limpiar</button>
                                    </div>
                                </div>
                            </div>

                    </div>
                   
                </div>

                <!----HASTA AQUI CUT--->
                <!-- INICIA ANEXAR DOCUMENTOS -->
               
            </div><!-- FIN contenido -->




            @*COMIENZO TABLA*@
            <div class="col-xs-12 col-md-12">

                <div id="div-tabla" class="table-responsive margen">
                    <h2>Lista de expedientes asignados para evaluación</h2>
                    <table id="tablaMpExpOA" class="table table-bordered table-hover table-striped" style="width:100%">
                        <thead>
                            <tr>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:2%; max-width:3%">Nro:</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">RUC</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:10%; max-width:15%">Razon social </th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Línea de acción </th>
                                
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Nro. Expediente</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Tipo incentivo</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">N° CUT</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:12%; max-width:15%">Especialista</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estado actual</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Fecha asignación</th>
                      
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Datos SDA/PRPA</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Estructura de inversión</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Programación de actividades</th>
                                <th rowspan="2" style="background-color:darkseagreen; color:black; text-align:center; vertical-align:middle; min-width:5%; max-width:5%">Plan de producción</th>

                            </tr>

                        </thead>
                        <tbody id="tbodyMPExpOA" class="tbodyMPExpOA">

                            <tr>
                                <td class="center">1</td>
                                <td class="center">20506759120 </td>
                                <td style="text-align:center"> Teps Group SAC </td>
                                <td class="center"> Tecnología </td>
                                <td class="center"> -- </td>
                            
                                <td class="center"> 020056748 </td>
                                <td class="center"> 201956247 </td>
                                <td class="center"> 20/10/2019 </td>
                                <td class="center">15/10/2019</td>
                                <td class="center">José Jimenez </td>
                                <td class="center">
                                    <button id="btnRegistraSDA_PRPA" class="btn btn-success btn-sm"><i class="ace-icon fa fa-pencil"></i></button>

                                </td>
                                <td style="text-align:center"> 
                                
                                    <button id="btnRegistraEstructura" class="btn btn-success btn-sm"><i class="ace-icon fa fa-pencil"></i></button>
                                 </td>
                                <td class="center">
                                    <button id="btnRegistrarProgramacion" class="btn btn-success btn-sm"><i class="ace-icon fa fa-pencil"></i></button>
                                </td>
                                <td class="center">
                                    <button id="btnRegistrarPlanProduccion" class="btn btn-success btn-sm"><i class="ace-icon fa fa-pencil"></i></button>
                                </td>

                            </tr>

                        </tbody>

                    </table>
                </div>

            </div>

            <!-- PANTALLA REGISTRA EXTRUCTURA -->
            <!-- MODAL REGISTRA PROGRAMA ACTIVIDADES -->
            <div class="modal" id="registraEstructura" tabindex="-1" role="dialog" aria-labelledby="recepcionarLabel" aria-hidden="true" style="overflow-y: auto;">
            <div class="modal-dialog modal-lg">
                <div class="modal-content" style="width:100%">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title">Recepcionar S.D.A.</h4>
                    </div>
                    <div class="modal-body" style="max-height:90%">

                        <div class="col-xs-12 col-md-12" id="recepcionarSDA">
                            <div id="accordion" style="border:1px solid;">

                                <div class="card">

                                    <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                                        <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                            <a style="color:white; text-decoration:none;">
                                                <i class="ace-icon fa fa-file"></i>
                                                &nbsp; DATOS GENERALES DE LA OA
                                            </a>
                                        </h5>
                                        <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                                            <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                                                <i class="ace-icon fa fa-chevron-down"></i>
                                            </a>
                                        </div>
                                    </div>

                                    <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">

                                        <div class="card-body" style="padding-left:12px;padding-right:12px;">
                                            <br />
                                            <div class="row" style="margin-bottom: 30px;">
                                                <div class="col-xs-11 col-md-11" style="text-align: left; border-bottom: 1px solid #ede9e9; background-color: lightgray">
                                                    <label style="text-align:left;">
                                                        DATOS GENERALES DE LA OA
                                                    </label>
                                                </div>
                                            </div>

                                            <div class="row" style="margin-bottom:5px;">
                                                @*margin-bottom: 5px;*@

                                                <div class="col-xs-12 col-md-4" style="text-align:left;">
                                                    <div>
                                                        <label style="text-align:left;">  RUC : </label>
                                                    </div>
                                                    <div>
                                                        @Html.TextBox("idJuntaDirectivaf", (string)"", new { style = "width:40%; display:none;" })
                                                        @Html.TextBox("rucOAf", (string)"", new { style = "width:40%", disabled = true, Value = @Session["RucOA"] })
                                                    </div>
                                                </div>



                                                <div class="col-xs-12 col-sm-12 col-md-4" style="text-align: left; ">
                                                    <div><label style="text-align:left;"> Razón Social : </label></div>
                                                    <div>
                                                        @Html.TextBox("razonSocial", (string)"", new { @class = "campoTexto", @MaxLength = "250", style = "width:90%", disabled = "disabled" })
                                                        <p id="ARazSocial" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo texto en este campo.</p>
                                                    </div>
                                                </div>
                                                @*margin-bottom: 5px;*@


                                            </div>

                                        </div>

                                    </div>

                                </div>

                            </div>
                        </div>
  

                    </div>
                </div>
            </div>
        </div>





        </div>

    </div><!-- fin contenedor -->

    
    @*FIN DE TABLA*@

</div><!-- fin contenido-->
                    </div>

<script type="text/javascript">

    $(document).ready(function () {
         
        controles_SDA();

         

    });

</script>