
@{
    ViewBag.Title = "adjuntarDocSDA";
    Layout = "~/Views/Shared/_LayoutMenuUn.cshtml";
}


<div class="main-content">
    <!-- contenedor -->

    <div class="main-content-inner">
        <div class="breadcrumbs ace-save-state" id="breadcrumbs">
            <span>Bienvenido  Sr: @Session["NombreUsua"]   </span>
            <!-- Fin Ruta -->
            <ul class="breadcrumb">
                <li>
                    <i class="ace-icon fa fa-home home-icon"></i>
                    <a href="/Modulos/">Modulos</a>
                </li>
                <li><a href="/UNegocios/index"> U.Negocios</a></li>
                <li class="active">Adjuntar Documento S.D.A.</li>
            </ul>

        </div><!-- Inicio ruta -->

        <div class="page-content">
            <!-- contenido -->
            </div>

        <div class="page-header">
            <!-- Inicio titulo -->

            <div class="cbp-mc-column">

                <h1> Adjuntar Documentos S.D.A.</h1>
            </div>
        </div>       <!-- Fin titulo -->


        <div class="col-xs-12 col-md-12">

            <div id="accordion" style="border:1px solid;">

            <div class="card">

                <div class="widget-header card-header" id="headingOne" style="text-align:left;">
                    <h5 class="widget-title" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <a style="color:white; text-decoration:none;">
                            <i class="ace-icon fa fa-search"></i>
                            &nbsp; BUSQUEDA
                        </a>
                    </h5>
                    <div class="widget-toolbar" data-toggle="collapse" data-target="#collapseOne" aria-expanded="true" aria-controls="collapseOne">
                        <a href="#collapseOne" data-action="collapse" style="color:#fff;">
                            <i class="ace-icon fa fa-chevron-down"></i>
                        </a>
                    </div>
                </div>

                <div id="collapseOne" class="collapse" aria-labelledby="headingOne" data-parent="#accordion">
                    <div class="card-body" style="padding-left:12px;padding-right:12px;">
                        <br />


                        <!----- VALIDA RUC ----->

                        <div class="row" style="margin-bottom:5px;">
                            <form class="card-body" style="padding-left:25px;padding-right:25px;">

                                <div class="row" style="margin-bottom:10px; text-align:left;">

                                <div class="col-xs-12 col-sm-12 col-md-1" style="text-align: left;">

                                    <label style="text-align:left"><strong> Buscar por:</strong></label>

                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-2" style="text-align: left;">

                                    <div class="radio">

                                        <label for="text" class="col-lg-12 col-form-label align-right width-35">
                                            <input type="radio" name="optionsRadios" id="rdbRucFl" value="option1" checked>
                                            RUC:
                                        </label>
                                    </div>

                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-2" style="text-align: left;">

                                    <div class="radio">
                                        <label for="text" class="col-lg-12 col-form-label align-left width-45">
                                            <input type="radio" name="optionsRadios" id="rdbSgdCutFl" value="option2" style="text-align:left">
                                            N° SGD_CUT:
                                        </label>
                                    </div>

                                </div>
                        </div>


                        @*margin-bottom: 5px;*@

                        @using (Html.BeginForm())
                        {
                            @*<div class="row" style="margin-bottom:5px;">*@

                            <div class="row" style="margin-bottom:10px;">

                                <div class="col-xs-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                    <div> <label style="text-align:left; width:20%">R.U.C :</label></div>
                                  
                                    <div>
                                        @Html.TextBox("nroRucFl", (string)"", new { @class = "campoNumero", @style = "width:50%", disabled = true, Value = @Session["RucOA"] })
                                        <button type="button" class="btn btn-info" id="btnBuscarRUC" title="Validar"> <i class="ace-icon fa fa-search"></i></button><br />
                                        <p id="AnroRucFl" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                    <div> <label style="text-align:left; width:30%">No SGD (CUT):</label></div>

                                    <div>
                                        @Html.TextBox("NroSgdCutFl", (string)"", new { @class = "campoNumero", @style = "width:50%", disabled = true, Value = @Session["RucOA"] })

                                        <p id="ANroSgdCutFl" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                    </div>
                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div><label style="text-align:left; width:50%">No Expediente:</label></div>
                                    @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                    <div><select id="cbxNroExpedienteOAFl" class="custom-select" style="width:50%;"> </select></div>

                                </div>

                                <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                    <div>
                                        <label style="text-align:left; width:50%">Nº SGD (CUT):</label>
                                    </div>

                                    @*@Html.TextBox("nroSGD", (string)ViewBag.bus_ruc, new { @class = "campoTexto", @style = "width:30%" })*@
                                    <div>
                                        <select id="cbxNroSGD" class="custom-select" style="width:50%;"> </select>
                                    </div>
                                </div>


                               
                            </div>

                                    <div class="row" style="margin-bottom:10px;">
                                        <div class="col-xs-12 col-md-6" style="text-align: left; margin-bottom: 5px;">
                                           <div><label style="text-align:left; width:20%">Razón Social :</label></div>
                                       <div>    @Html.TextBox("razonSocial", (string)"", new { @MaxLength = "500", @style = "width:65%", disabled = true })
                                        </div>
                                        </div>

                                        <div class="col-xs-12 col-md-6" style="text-align: left; margin-bottom:5px;">
                                            <div><label style="text-align:left; width:20%">Tipo Incentivo:</label></div> 
                                            <div>  @Html.TextBox("tipoIncentivo", (string)"", new { @MaxLength = "500", @style = "width:65%", disabled = true })</div> 
                                        </div>

                                    </div>
                                                <!-- columna  3 -->

                                    <div class="row" style="margin-bottom:10px;">
                                        <div class="col-xs-12 col-md-6" style="text-align: left; margin-bottom: 5px;">
                                            <div><label style="text-align:left; width:20%">Nombre de PNT / PRPA:</label></div> 
                                            <div> @Html.TextBox("nombrePNT_PRPA", (string)"", new { @MaxLength = "500", @style = "width:65%", disabled = true })</div> 
                                        </div>

                                        <div class="col-xs-12 col-md-6" style="text-align: left; margin-bottom:5px;">
                                            <div><label style="text-align:left; width:20%">Especialista Asignado:</label></div>
                                            <div> @Html.TextBox("espAsignado", (string)"", new { @MaxLength = "500", @style = "width:65%", disabled = true })</div>
                                        </div>
                                    </div>

                        }

                        </form>
                    </div>


                        <div class="row">

                            <div class="cbp-mc-column " style="align-content:center">
                                <br>
                                <button id="btnConsultarUsuarioAp" class="btn btn-success" style="margin-bottom:25px"><i class="ace-icon fa fa-search"></i>BUSCAR</button>
                                                              
                            </div>

                        </div>


                        <!-- --------- PERSONA CONTACTO -------- -->

                        <div class="row">
                            <div class="col-xs-12 col-md-11" style="text-align: left; margin-bottom: 1%; border-bottom: 1px solid #ede9e9;">
                                <h3>
                                    <i class="ace-icon fa fa-file"></i>
                                    &nbsp; ADJUNTAR DOCUMENTO
                                </h3>
                            </div>
                        </div>

                        <br />


                        <div class="row" style="margin-bottom:10px; margin-left:auto;">

                            @*margin-bottom: 5px;*@

                            <div class="col-xs-12 col-md-3" style="text-align:left; margin-bottom: 5px;">
                                <div>
                                    <div> <label style="text-align:left; width:35%"> RUC: </label></div>

                                    <div> @Html.TextBox("rucOAf", (string)"", new { style = "width:50%", disabled = true, Value = @Session["RucOA"] })</div> 
                                </div>
                            </div>

                            @*margin-bottom: 5px;*@

                            <div class="col-md-3" style="text-align: left; margin-bottom: 5px;">

                                <div> <label style="text-align:left; width:35%"> N° de Expediente:</label></div> 

                                <div>  @Html.TextBox("nroExp", (string)"1234-2018", new { @class = "solo-numero", style = "width:50%", disabled = "true" })</div> 

                            </div>

                            <div class="col-xs-12 col-md-3" style="text-align: left; margin-bottom: 5px;">
                                <div>   <label style="text-align:left; width:35%">No SGD (CUT):</label> </div>                            
                                <div>
                                    @Html.TextBox("nroSgdCut", (string)"", new { @class = "campoNumero", @style = "width:50%", disabled = true, Value = @Session["RucOA"] })
                                    <br />
                                    <p id="nroSgdCut" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                </div> 
                            </div>


                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <div> <label style="text-align:left; width:35%">Unidad PCC:</label></div>
                                    @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                    <div> <select id="cbxUnidadPCCFl" class="custom-select" style="width:50%;"> </select></div>  
                                </div>
                            </div>

                        </div>


                        <div class="row" style="margin-bottom:10px; margin-left:auto;">

                            <div class="col-xs-12 col-md-3" style="text-align:left; margin-bottom: 5px;">
                                <div>
                                    <label style="text-align:left; width:45%"> Estado Actual PN/PRPA: </label></div>

                                  <div>
                                    @Html.TextBox("estadoActual", (string)"", new { style = "width:50%", disabled = true, Value = @Session["RucOA"] })
                                </div>
                            </div>

                            <div class="col-xs-12 col-md-3" style="text-align:left; margin-bottom: 5px;">
                                <div>
                                    <label style="text-align:left; width:35%"> Registrado por: </label></div>

                                <div>
                                    @Html.TextBox("registradoPor", (string)"", new { style = "width:50%", disabled = true, Value = @Session["RucOA"] })
                                </div>
                            </div>

                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <label style="text-align:left; width:35%">Oficina Regional:</label></div>

                                <div>    @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                    <select id="cbxOfiRegionalFl" class="custom-select" style="width:50%;"> </select>
                                </div>
                            </div>
                            
                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <label style="text-align:left; width:35%">Tipo Documento:</label> </div>
                                    @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                 <div>  <select id="cbxTipoDocFl" class="custom-select" style="width:50%;"> </select>
                                </div>
                            </div>

                           
                        </div>


                        <div class="row" style="margin-bottom:10px; margin-left:auto;">

                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <label style="text-align:left; width:35%">Nro. Doc. Adjunto:</label></div>

                                 <div>  @Html.TextBox("NroDocAdjFl", (string)"", new { @class = "campoNumero", style = "width:50%" })
                                    <p id="ANroDocAdjFl" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numéricos en este campo.</p>
                                </div>
                            </div>

                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <label style="text-align: left; width:35%">Referencia: </label></div>

                                <div>     @Html.TextBox("referenciaDoc", (string)" ", new { @class = "campoGenerico", @maxlength = "250", style = "width:50%" })
                                    <p id="AreferenciaDoc" class="alerta" style="display:none; font-family:'Gill Sans'; color:darkred">Solo valores numericos en este campo.</p>
                                </div>
                            </div>


                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left;">
                                <div>
                                    <label style="text-align: left; width:35%"> Asunto: </label></div>

                                 <div>   @Html.TextBox("asuntoDoc", (string)"", new { @class = "campoGenerico", @maxlength = "250", style = "width:50%" })
                                </div>
                            </div>

                            <div class="col-xs-12 col-md-3" style="text-align: left; margin-top: 10px;">
                                <div>
                                    <label style="text-align: left; width:45%;"> Fecha Doc. Adjunto: </label>
                                </div>

                                <div>
                                    <div class="input-group date" style="width: 45%; Text-align: left; margin-top:-30px; margin-left:180px;">
                                        @Html.TextBox("FechaDocAdj", (string)"", new { @class = "datetimepicker", @MaxLength = "50", style = "width:100%; Text-align:left;" })
                                        <span class="input-group-addon" style="text-align:left;">
                                            <span class="glyphicon glyphicon-calendar"></span>
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <br />

                        <div class="row" style="margin-bottom:10px; margin-left:auto;">

                            <div class="col-xs-12 col-sm-12 col-md-3" style="text-align: left; margin-bottom:20px;">
                                <div>
                                    <label style="text-align:left; width:45%">Visualiza el Documento:</label></div>
                                    @*@Html.TextBox("nroExp", (string)"", new { @class = "campoTexto", @style = "width:30%" })*@
                                  <div> <select id="cbxVisualizaDoc" class="custom-select" style="width:50%;"> </select>
                                </div>
                            </div>

@*SELECCIONAR ARCHIVO*@      <div class="col-xs-12 col-sm-12 col-md-9" style="text-align: left; margin-bottom:40px;">
                                  
                                    <div class="col-xs-12 col-sm-12 col-md-9" style="text-align: left;">
                                        @Html.TextBox("nro_orden", (string)"", new { type = "file", style = "width:50%" })
                                       
                                    </div>
                            </div> 
                        </div>

                        <div class="row" style="margin-bottom:10px;">
                            <div class="col-xs-12 col-sm-12 col-md-12" style="text-align: center;">

                                <button id="btnGuardarDoc" style="margin-top:-50px;" type="button" class="btn btn-success" data-dismiss="modal"><i class="ace-icon fa fa-floppy-o"> </i> GUARDAR</button>
                                <a> &nbsp;</a>
                                <button id="btnLimpiarFiltro" class="btn btn-success" style="margin-bottom:25px; margin-top:-25px;"><i class="ace-icon fa fa-eraser"></i>LIMPIAR</button>
                            </div>
                            </div>

                            @*//diiv contendeor*@
                        </div>
                    <br />

                    <div class="col-xs-12">
                        <br />
                        <h2>Listado de Documentos Adjuntos</h2>
                    <!-- inicio tabla-->
                    <div class=" table-responsive margen">
                        <table id="tab_opa" class="table  table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th rowspan="2" width="3%" style="background-color:darkseagreen; color:black;" class="center">Nº</th>
                                    <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">N° SGD(CUT)</th>
                                    <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Tipo Documento</th>
                                    <th rowspan="2" width="15%" style="background-color:darkseagreen; color:black;" class="center">Asunto</th>
                                    <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">N° Documento</th>
                                    <th rowspan="2" width="20%" style="background-color:darkseagreen; color:black;" class="center">Referencia</th>
                                    <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Fecha Documento</th>
                                    <th rowspan="2" width="10%" style="background-color:darkseagreen; color:black;" class="center">Ver Documento</th>
                                    <th rowspan="2" width="7%" style="background-color:darkseagreen; color:black;" class="center">Visualizado por OA</th>
                                    <th colspan="2" width="12%" style="background-color:darkseagreen; color:black;" class="center">Acciones</th>
                                    
                               </tr>
                                <tr>
                                    <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Modificar</th>
                                    <th class="text-center" style="background-color:darkseagreen; color:black; min-width:5%; max-width:5%">Eliminar</th>
                                </tr>
                            </thead>

                            <tbody>
                                <tr>
                                    <td class="center">1</td>
                                    <td class="center">CAFE </td>
                                    <td style="text-align:left"> AGRICOLA </td>
                                    <td class="center"> ACTIVO </td>
                                    <td class="center"> ACTIVO </td>
                                    <td class="center"> ACTIVO </td>
                                    <td class="center"> ACTIVO </td>
                                    <td class="center"> 
                                        <a class="btn btn-primary" id="" href="#modificarModal" title="Modificar" data-toggle="modal">
                                            <i class="ace-icon fa fa-eye"></i>
                                        </a>
                                    
                                    </td>
                                    <td class="center"> ACTIVO </td>
                                    <td class="center">
                                        <a class="btn btn-warning" id="" href="#modificarModal" title="Modificar" data-toggle="modal">
                                            <i class="ace-icon fa fa-edit"></i>
                                        </a>
                                    </td>
                                    <td class="center">
                                        <a> &nbsp;</a>
                                        <a class="btn btn-danger" id="eliminarBtn" href="#eliminarModal" title="Eliminar" data-toggle="modal">
                                            <i class="ace-icon fa fa-trash"></i>
                                        </a>
                                    </td>
                                </tr>      
                            </tbody>
                        </table>
                    </div>

                </div>

@*FIN DE TABLA*@

                </div>
            </div>

            <!---- Fin Datos del Contacto ----->
        </div>
        
        
        </div>

        </div>
</div>

